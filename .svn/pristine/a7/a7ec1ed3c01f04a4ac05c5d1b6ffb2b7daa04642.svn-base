<div *ngIf="selectOrgModalShown" class="modal-md modal fade" [config]='promptConfig' (onHidden)="onHideSelectOrg()"
    bsModal #autoShownModal2="bs-modal" tabindex="-1" role="dialog" aria-labelledby="dialog-auto-name">
    <div class="modal-dialog">
        <div class="modal-content">
            <label class="control-label" style="margin: 15px 0px 9px 15px;">Choose Current Organization</label>
            <hr style="border: 0; border-top: 2px solid #eee; margin-top:0px; margin-bottom:0px">
            <div class="modal-body" style="padding-bottom: 0px">

                <div class="row">
                    <div class="col-lg-12 cp margin-top-12">
                        <!-- <div class="promp-align1"> -->
                        <div class="row">
                            <div class="col-sm-2 promp-align1">
                                Note:
                            </div>
                            <div class="col-sm-10 promp-align1" style="padding-left: 0px;
                               margin-left: -39px;">
                                <!-- 'Medical/ Healthcare professional' or 'Administrative Healthcare personnel' -->
                                <p>
                                    For a professional category user type at least one current organization is
                                    mandatory and one organization should be chosen to be shown on your profile. Please
                                    choose one from
                                    your work experience below.
                                </p>
                            </div>
                        </div>
                        <!-- </div> -->
                    </div>
                </div>

                <!-- (typeaheadNoResults)="changeTypeaheadNoResults(i, 'currentOrg')" -->
                <form [formGroup]="currentOrgForm" class="app-form">
                    <div class="row">
                        <div class="col-sm-12">
                            <input placeholder="Choose one current organization" class="form-control input-sm max-hight"
                                formControlName="chooseCurrentOrg" (typeaheadOnSelect)="onCurrentOrgChosen($event)"
                                [typeahead]="currentOrgList" [typeaheadMinLength]="0"
                                typeaheadOptionField="chooseCurrentOrg" [typeaheadItemTemplate]="customItemTemplate4"
                                autocomplete="off" [typeaheadOrderBy]="sortConfigWork">

                            <ng-template #customItemTemplate4 let-model="item" style="width: 41rem !important;">
                                <span>
                                    <strong style="font-size: 14px;">
                                        {{model.chooseCurrentOrg}}
                                    </strong>
                                    <br>
                                    <!-- Italicized location details -->
                                    <i>
                                        <small>
                                            {{model.street }}<span *ngIf="model.street">{{", "}}</span>
                                            {{model.city}}<span *ngIf="model.city">{{", "}}</span>
                                            {{model.state}}<span *ngIf="model.state">{{", "}}</span>
                                            {{model.country}}
                                        </small>
                                    </i>
                                </span>
                            </ng-template>
                            <div *ngIf="currentOrgSaveFlag &&
                                        createControl.chooseCurrentOrg.invalid &&
                                        (createControl.chooseCurrentOrg.dirty || createControl.chooseCurrentOrg.touched)"
                                class="error-msg">
                                <div *ngIf="createControl.chooseCurrentOrg.errors.required"> This field is required
                                </div>
                            </div>
                        </div>
                    </div>
                </form>

                <div class="row pad-buttons">
                    <button (click)="saveCurrentOrg()" class="btn primary-btn btn-sm">Save</button>
                </div>
            </div>
        </div>
    </div>
</div>


