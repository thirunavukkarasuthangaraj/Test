                                                <div
                                                    *ngIf="getWorkFormGroup(i).controls['zipcode'].hasError('required')">
                                                    Zipcode is required</div>
                                                <div
                                                    *ngIf="getWorkFormGroup(i).controls['zipcode'].hasError('pattern')">
                                                    Please enter only numbers</div>
                                            </div>
                                        </div>
                                        <div class="col-sm-6" *ngIf="workExperience.value[i].country=='US'">
                                            <input disabled type="text" placeholder="State" formControlName="state"
