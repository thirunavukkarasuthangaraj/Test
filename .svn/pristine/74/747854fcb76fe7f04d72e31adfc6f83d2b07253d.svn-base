import { UtilService } from 'src/app/services/util.service';
import { ActivatedRoute } from '@angular/router';
import { Component, OnInit } from '@angular/core';
import { ApiService } from 'src/app/services/api.service';

@Component({
  selector: 'app-directory-profile',
  templateUrl: './directory-profile.component.html',
  styleUrls: ['./directory-profile.component.scss']
})
export class DirectoryProfileComponent implements OnInit {
  data:any;
  similarprofile:any;
  profileId;
  constructor(private aroute: ActivatedRoute, private util: UtilService, private api: ApiService) {
    this.aroute.params.subscribe(res => {
      this.profileId = res.profileId;
    });
  }

  ngOnInit() {
    this.getprofileData();
    this.getsimilarprofileData();
  }


  route(id){
    this.profileId =id;
    this.getprofileData(); 
  }
 

  getprofileData() {
    this.util.startLoader()
    this.api.query('healthcare/hcp/profile/' + this.profileId).subscribe(res => {
      this.util.stopLoader()
      if (res != null) {
        if (res.code == "00000") {
          if (res.data.hcpprofile != null && res.data.hcpprofile != undefined) {
            this.data = res.data.hcpprofile;
          }
        }
      }
    },err => {
      this.util.stopLoader(); 
    })
  }
  getsimilarprofileData() {
   
    this.api.query('healthcare/hcp/similar/profile/' + this.profileId).subscribe(res => {
       if (res != null) {
        if (res.code == "00000") {
          if (res.data.similarprofile != null && res.data.similarprofile != undefined) {
            this.similarprofile = res.data.similarprofile;
          }
        }
      }
    },err => {
      this.util.stopLoader(); 
    })
  }

}
