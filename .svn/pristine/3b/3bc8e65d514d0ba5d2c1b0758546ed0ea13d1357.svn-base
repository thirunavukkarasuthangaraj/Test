import { Injectable } from '@angular/core';
import { Observable, Subject } from 'rxjs';

@Injectable({
  providedIn: 'root',
})

export class ProfilePhoto {
    constructor(){}
    subject = new Subject<any>();

    setPhoto(value){
        this.subject.next(value)
    }

    getPhoto(): Observable<any>{
        return this.subject.asObservable()
    }
}