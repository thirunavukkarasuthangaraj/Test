<li class="profile-memeber-container margin-bottom-5">
  <a class="profile-view">
    <img [src]="img" width="56" onerror="src='assets/images/userAvatar.png'" height="56" class="profile-photo" (click)="route(userinfo)"
      [ngClass]="{ business: type === 'BUSINESS' || type === 'COMMUNITY' || type === 'POST'
      || type === 'HASHTAG' || type === 'JOBS'}" />

    <div class="profile-detail" (click)="route(userinfo)">
      <h3 class="profile-section-detail">
        <span class="profile-name card-title-font">
          <ng-content select="[headline1]" ></ng-content>
        </span>
      </h3>
      <p class="profile-headline">
        <span class="profile-inline card-subtitle-font">
          <ng-content select="[headline2]"></ng-content>
        </span>
      </p>
    </div>
  </a>
  
  <div class="profile-btn">
    <ng-content select="[btn]"  (click)="datapass(userinfo.userId)"></ng-content>
  </div>
</li>
