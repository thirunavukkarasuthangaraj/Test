<div class="business-banner-top post-box-shadow">
  <div class="business-banner">
    <div class="banner-shadow"></div>
    <img class="banner-img" [ngStyle]="{'background-image': 'url(' + bannerimg.src + ')'}">
    <div class="banner-overlay">
      <div class="banner-edit" title="Edit banner" (click)="uploadBannerHere()" style="cursor: pointer;" *ngIf="commonVariables?.localUser">
        <input type="file" style="display: none!important;" accept="image/jpg, image/jpeg, image/png" #myFileInput title="Edit banner"
          (change)="fileChangeEvent($event,template)" />
        <div class="image-upload"></div>
        <i class="fa fa-camera" aria-hidden="true"></i>
      </div>
      <div title="Change or remove picture" (mouseenter)="showImage=true" (mouseleave)="showImage=false" *ngIf="commonVariables.localUser" yPosition="below" xPosition="after" [matMenuTriggerFor]="menu" class="banner-profile-image" style="position: absolute; bottom: -25px;" >
        <img class="img-small1" src="{{profilePhoto.src}}" onerror="this.src='assets/images/userAvatar.png';">


          <mat-menu #menu="matMenu" >
            <input #myFileInput2  style="display: none;" type="file" (click)="myFileInput2.value=null" (change)="fileChangeEvent2($event, profilePhotoUpload)"
            accept="image/jpg, image/jpeg, image/png"  />
            <button   class="wqmzi" mat-menu-item (click)="uploadPhotoHere()">
              <span class="ixym"><i class="far fa-user-circle fa-lg"></i></span>
              <span class="ixym kjp"><i class="fas fa-user"></i></span>
              <span class="skpax">Change profile photo</span>
            </button>
            <button  type="button" (click)="openSweetalert()" class="wqmzi" mat-menu-item>
              <span class="ixym"><i class="fas fa-user-slash"></i></span>
              <span>Remove profile photo</span>
            </button>
            <!-- <button  class="wqmzi" mat-menu-item>
              <span class="ixym"><i class="far fa-user-circle fa-lg"></i></span>
              <span>View profile photo</span>
            </button> -->
          </mat-menu>
          <span class='sssld'>
            <i  *ngIf="showImage==true" class="fa fa-camera"  aria-hidden="true"></i>
          </span >
        </div>



      <div class="banner-profile-image1" *ngIf="!commonVariables.localUser" style="position: absolute; bottom: -25px;">
        <img class="img-small1" src="{{profilePhoto.src}}" onerror="this.src='assets/images/userAvatar.png';">
      </div>

      <!-- <button mat-icon-button [matMenuTriggerFor]="menu" aria-label="Example icon-button with a menu">
        <mat-icon>more_vert</mat-icon>
      </button> -->

    </div>
  </div>

  <div class="row business-details1" style="word-wrap: break-word;">
    <div class="col-md-8" style="margin: 30px 0px 0px;">
      <div *ngIf="this.infoData.userType !== 'student'">
        <p *ngIf="infoData.firstName && infoData.lastName" class="profile-name ellipse2" title="{{infoData.firstName + ' ' + infoData.lastName}}" data-toggle="tooltip"
          data-placement="top">
          {{infoData.firstName + " " + infoData.lastName}}
        </p>
        <p *ngIf="infoData.pitch" class="profile-tag ellipse1" data-toggle="tooltip" data-placement="top">
          {{infoData.pitch}}
        </p>

        <p *ngIf="experelement.title" class="profile-others ellipse1" title="{{experelement.title}}">
          {{experelement.title}}
        </p>

        <p *ngIf="experelement.organisationName" class="profile-others ellipse1" title="{{experelement.organisationName}}">
          {{experelement.organisationName}}
        </p>

        <p *ngIf="infoData.city && infoData.state" class="profile-others ellipse1" title="{{infoData.city + ', ' + infoData.state}}">
          {{infoData.city + ', ' + infoData.state}}
        </p>
      </div>



      <div *ngIf="this.infoData.userType === 'student'">
        <p *ngIf="infoData.firstName && infoData.lastName" class="profile-name ellipse2" title="{{infoData.firstName + ' ' + infoData.lastName}}" data-toggle="tooltip"
          data-placement="top">
          {{infoData.firstName + " " + infoData.lastName}}
        </p>
        <p *ngIf="infoData.pitch" class="profile-tag ellipse" data-toggle="tooltip" data-placement="top">
          {{infoData.pitch}}
        </p>

        <p class="profile-others ellipse">
          Student
        </p>
        <div class="col-sm-1">
        </div>
        <p *ngIf="educationElement.schoolName" class="profile-others ellipse" title="{{educationElement.schoolName}}">
          <span>
            <i class="fas fa-user-graduate "></i>
          </span>
          {{educationElement.schoolName}}
        </p>

        <p *ngIf="educationElement.city && educationElement.state" class="profile-others" title="{{educationElement.city + ', ' + educationElement.state}}">
          {{educationElement.city + ', ' + educationElement.state}}
        </p>
      </div>












    </div>

    <div class="col-md-4">
      <div class="pull-right row ">
        <div class="col-md-1" *ngIf="commonVariables?.localUser">
