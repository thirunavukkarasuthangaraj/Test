<ng-template #photoDeleteTemplate>
  <button type="button" style="font-size: 31px;padding-right: 17px;padding-top: 7px;" class="close pull-right"
    aria-label="Close" (click)="no()">
    <span aria-hidden="true">&times;</span>
  </button>
  <label style="margin: 6px 10px 14px 10px;padding-top:10px">Remove Profile Photo</label>
  <hr style="margin: 0px !important;">
  <div class="modal-body ">
    <div class="text-left">
      <p>Are you sure you want to delete the profile photo?</p>
    </div>
    <div class="text-right">
    <button type="button" class="btn btn-default" (click)="no()" >No</button>
    <button type="button" class="btn btn-primary" (click)="yes()" style="margin-left: 0.5rem;">Yes</button>
    </div>
  </div>
</ng-template>
