<!-- <app-langding-page-nav-bar></app-langding-page-nav-bar> -->
<div class="msg-page app-content  post-page-background">
    <div class="container">
        <div class="row">
            <div class="col-md-2" [class.stick-there]="stickThere" #stickedThere style="padding-right: 0px;">
                <div class="hpanel">
                    <div class="panel-body" style="padding: 0px !important">
                        <div class="holder">
                            Notifications<span class="badge noti-badge"
                                *ngIf="notificationcount > 0">{{notificationcount}} </span>
                            <span *ngIf="notificationcount > 0"> (New)</span>
                        </div>
                        <div class="users-list" [style.height.px]="chatHeight">
                        </div>
                    </div>
                </div>
            </div>
            <!-- <div class="col-md-10"> -->
            <!-- <div class="row"> -->
            <div class="col-md-7" style="padding: 0px 5px 0px 5px;"  infiniteScroll [infiniteScrollDistance]="5" [infiniteScrollThrottle]="1500"
            (scrolled)="onScrollDown()">

                <div class="hpanel ">
                    <!-- panel-body no-padding -->
                    <div class="panel-body no-padding" style="min-height: 600px;">
                        <div class="wert"  id="jingalala" style="visibility: hidden;" *ngIf="commonVariables.notificationList === undefined || commonVariables.notificationList.length === 0">
                            <div class="col-md-12 mg-bt-9">
                                <img style=" max-height: 20rem;" src="assets/icon/notification_no.png" alt="">
                            </div>
                            <div class="col-md-12">
                                <p class="font-16">You have got no notifications</p>
                            </div>
                            <div class="col-md-12">
                                <p class="biase">Please, check this page when you have one</p>
                            </div>
                        </div>
                        <div *ngFor="let item of commonVariables.notificationList; let i=index; let lastItem=last;">
                            <!-- notification type for employee request rejected -->
                            <!-- BUSINESS_EMPLOYEE_REQUEST_REJECTED -->
                            <!-- [ngClass]="{'bg-wt': item.read, 'bg-pale': item.unread}" -->
                            <div [ngClass]="{'bg-pale': !item.read, 'bg-wt': item.read}"
                                *ngIf="item['notifyType'] === 'BUSINESS_EMPLOYEE_REQUEST_REJECTED'">
                                <div class="row contain">
                                    <div class="col-md-4">
                                        <div class="row">
                                            <div class="col-md-3">
                                                <img *ngIf="item['notifyData']['logo']==null"
                                                    (click)="routeToPageOrProfile(item, 'businessPage')"
                                                    class="img pic-size" src="assets/images/gallery/company.png" alt="">
                                                <img *ngIf="item['notifyData']['logo']!=null" class="img pic-size"
                                                    src="{{item['notifyData']['logo']}}" alt=""
                                                    (click)="routeToPageOrProfile(item, 'businessPage')">
                                            </div>
                                            <div class="col-md-9 no-pad-rt">
                                                <div class="letter-gaps break-word solo-text hov-txt" data-toggle="tooltip"
                                                    data-placement="top"
                                                    (click)="routeToPageOrProfile(item, 'businessPage')"
                                                    title="{{item['notifyData']['businessName'] }}">
                                                    {{item['notifyData']['businessName'] | truncate:22 }}
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-5 xr">
                                        <div class="row">
                                            <div class="col-md-12"> 
                                                <div class="letter-gaps">
                                                    {{item['notifyData']['businessName'] | truncate:30 }} your work experience verification failed.
                                                </div>
                                            </div>

                                            <!-- <div class="col-md-12" style="float: right;">
                                                <button class="btn  btn-sm btn-special"
                                                    (click)="redirectToSaidPage(item)">View</button>
                                            </div> -->
                                        </div>
                                    </div>
                                    <div class="col-md-3">
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div class="btn-group three_icon_cmon1 hovers" dropdown>
                                                    <a id="button-animated" dropdownToggle>
                                                        <img src="assets/icon/icons8-more-96.png" alt="" srcset=""
                                                            class="three-dot">
                                                    </a>
                                                    <ul id="dropdown-animated" *dropdownMenu class="dropdown-menu"
                                                        role="menu" style="right: 0;left: auto;"
                                                        aria-labelledby="button-animated">
                                                        <li *ngIf="item.notifyData.status=='REJECTED'" role="menuitem"
                                                            (click)="resendEmployeeRequest(item, i)">
                                                            <a>Resend Employee Request</a>
                                                        </li>
                                                        <li  role="menuitem" *ngIf="!item.read"
                                                            (click)="markAsRead(item, i)">
                                                            <a>Mark As Read</a>
                                                        </li>

                                                        <li role="menuitem"
                                                        (click)="redirectToSaidPage(item)">
                                                            <a>View</a>
                                                        </li>
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-sm-12" style="margin-bottom: -15px;">
                                        <div class="qwer">
                                            {{item.notifyTime}}
                                        </div>
                                    </div>
                                </div>
                                <hr class="no-mr">
                            </div>

                            <div [ngClass]="{'bg-pale': !item.read, 'bg-wt': item.read}"
                            *ngIf="item['notifyType'] === 'BUSINESS_PAGE_DEACTIVATED'">
                            <div class="row contain">
                                <div class="col-md-4">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <img *ngIf="item['notifyData']['logo']==null"
                                                class="img pic-size" src="assets/images/gallery/company.png" alt="">
                                            <img *ngIf="item['notifyData']['logo']!=null" class="img pic-size"
                                                src="{{item['notifyData']['logo']}}" alt="">
                                        </div>
                                        <div class="col-md-9 no-pad-rt">
                                            <div class="letter-gaps break-word solo-text hov-txt" data-toggle="tooltip"
                                                data-placement="top"
                                                title="{{item['notifyData']['businessName'] }}">
                                                {{item['notifyData']['businessName'] | truncate:22 }}
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-5 xr">
                                    <div class="row">
                                        <div class="col-md-12"> 
                                            <div class="letter-gaps">
                                                Business Page {{item['notifyData']['businessName'] | truncate:30 }} has been deactivated.
                                            </div>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <div class="row">
                                        <div class="col-sm-12">
                                            <div class="btn-group three_icon_cmon1 hovers" dropdown>
                                                <a id="button-animated" dropdownToggle>
                                                    <img src="assets/icon/icons8-more-96.png" alt="" srcset=""
                                                        class="three-dot">
                                                </a>
                                                <ul id="dropdown-animated" *dropdownMenu class="dropdown-menu"
                                                    role="menu" style="right: 0;left: auto;"
                                                    aria-labelledby="button-animated">
                                                    <li  role="menuitem" *ngIf="!item.read"
                                                        (click)="markAsRead(item, i)">
                                                        <a>Mark As Read</a>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-sm-12" style="margin-bottom: -15px;">
                                    <div class="qwer">
                                        {{item.notifyTime}}
                                    </div>
                                </div>
                            </div>
                            <hr class="no-mr">
                        </div>

