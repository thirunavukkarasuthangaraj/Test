      <!-- accout block massege start  -->
      <div *ngIf= "AccountBlockMassege">
        <div class="register-container" style="">
          <div class="row">
            <div class="col-md-8" style="background-color: white;">
              <div class="">
                <div class="text-center m-b-md">
                  <img class="app-logo" src="assets/images/CareOnline.jpg" width="35%">
                </div>
                <div class="panel-body">
                  <h5>Your Accout has been locked. Please contact Admin</h5>
                  <button class="btn btn-info btn-block" [routerLink]="['/login']" routerLinkActive="router-link-active" > thank you</button>
                  <div class="row">
                    <div class="col-md-12 text-center">
                      <strong>New to CareOnline?</strong> <a>Sign Up Free</a>
                    </div>
                </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end  -->

       <!-- accout block massege start  -->
       <div *ngIf= "MailSentMassege">
        <div class="register-container">
          <div class="row">
            <div class="col-md-12">
              <div class="">
                <div class="text-center m-b-md">
                  <img class="app-logo" src="assets/images/CareOnline.jpg" width="35%">
                </div>
                <div class="panel-body">
                  <div class="text-center">
                    <H3>Password reset link was sent to your email address</H3>
                  </div>
                  <!-- <H3>Password reset link was sent to your email address</H3> -->
                  <div class="text-center" style="font-size: 45px;">
                    <a [routerLink]="['/login']" routerLinkActive="router-link-active" style="color: black;" ><i class="pe-7s-mail" aria-hidden="true"></i></a>
                  </div>

                  <div style="margin-left: 7rem;" >
                    <p>Please follow the Instruction in the email to setup a new password</p><br>
                    <p>If you have not received an email please check your spam folder and re-enter the email address</p><br>
                    <p>If you need further assitance please submit a request through our support portal</p><br>
                  </div>
                  <!-- <button class="btn btn-success btn-block" [routerLink]="['/login']" routerLinkActive="router-link-active" > thank you</button> -->
                  <!-- <div class="row">
                    <div class="col-md-12 text-center">
                      <strong>New to CareOnline?</strong> <a>Sign Up Free</a>
                    </div>
                </div> -->
                </div>
              </div>
            </div>
          </div>
        </div>
      <!-- </div> -->
      <!-- end  -->


