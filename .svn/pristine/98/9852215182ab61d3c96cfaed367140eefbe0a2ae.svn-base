<div class="contanier1">
    <ngx-spinner
    bdColor="rgba(51,51,51,0.8)"
    size="medium"
    color="#fff"
    type="ball-scale-multiple"
    >
    <p style="font-size: 20px; color: white">Loading...</p>
    </ngx-spinner>
    <div class="color-line"></div>
    <div>
      <app-header></app-header>
    </div>
    <!-- <div class="color-line"></div> -->
    <app-admin-panel-alert></app-admin-panel-alert>
    <div class="inline" style="min-height: 100vh">
    <div>
         <div class="row" *ngIf = "captch">
        <!-- <div class="col-md-6">
          <img src="../../../assets/images/sign.png" width="70%" alt="" srcset="">
        </div> -->
        <div class="col-md-12">
            <div class="login-container">
                <div class="row">
                    <div class="col-md-12">
                       <div class="">
                            <div class="panel-body" style="background-color: white;">

                                  <div >
                                    <div class="text-center" style="margin-bottom:25px">
                                      <!-- <h3>CareOnline</h3>
                                      <small>Empowering You</small> -->
                                      <img class="app-logo" style="margin-bottom:25px" src="assets/images/CareOnline.jpg" width="50%">
                                      <p>
                                        Enter the email address you used to register with CareOnline
                                    </p>
                                    </div>
                                    <form [formGroup]="forgotpasswordForm" (ngSubmit)="forgotPassword()" class="custom-form">
                                      <!-- <form [formGroup]="forgotpasswordForm" (ngSubmit)="forgotPasswordAppuser()"> -->
                                        <div class="form-group">
                                            <label class="control-label" for="username">Email address</label>
                                            <input type="text" placeholder="Email address"   [disabled]="isDisabled" formControlName="recoverPassword" class="form-control">
                                            <div *ngIf="pwd.recoverPassword.invalid && (pwd.recoverPassword.dirty || pwd.recoverPassword.touched)"
                                                  >
                                              <div *ngIf="pwd.recoverPassword.errors.required" style="color: red;"> Please enter Email Address</div>
                                              <div *ngIf="pwd.recoverPassword.errors.email" style="color: red;"> Enter valid Email Address</div>
                                            </div>
                                        </div>
                                          <!-- <div style="margin-bottom: 1rem;" >
                                            <re-captcha (resolved)="resolved($event)"  siteKey="6Le4duAUAAAAABP4P5bbkAdF1tfMmV902iKsCoo2"></re-captcha>
                                          </div> -->
                                        <button class="btn primary-btn btn-sm btn-block" [disabled]="!forgotpasswordForm.valid" type="submit">Send</button>
                                    </form>
                                  </div>

                                    <div *ngIf ="forgetPasswordMsg">
                                      <h5 style="color: red;">Sorry, we don't recognise that email address</h5>
                                      <P>Do you want to <span style="color: blue; cursor: pointer" [routerLink]="['/signUp']" >Sign Up</span>?</P>
                                    </div>

                                    <div *ngIf = "msgsend">
                                      Please check the mail for Password
                                      <button class="btn btn-info btn-block" [routerLink]="['/login']" routerLinkActive="router-link-active" >Thank you</button>
                                    </div>
                                    <div *ngIf = "notActive">
                                      <h5 style="color: red; line-height: 1.5;">
                                        Account is not yet activated, please check your email to complete registration.
                                      </h5>
                                    </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

      </div>

       <!-- app user question start  -->
       <div *ngIf="appUserQ1">
         <div class="register-container">
          <div class="row">
            <div class="col-md-12">
              <div class="">
                <div class="text-center m-b-md">
                  <img class="app-logo" src="assets/images/CareOnline.jpg" width="35%">
                </div>
                <div class="panel-body">
                  <form [formGroup]="forgotPasswordAppUserForm" (ngSubmit)="AppuserQuestion()" class="custom-form" >
                    <div class="row">
                      <div class="text-center m-b-md">
                        <h4>Forgot your Password ?</h4>
                        <h6>Please Answer the Security Question</h6>
                      </div>
                      <div class="form-group col-lg-12">
                        <label>Email</label>
                        <input type="text"  formControlName="AppUserEmail" class="form-control" [attr.disabled]="true">
                        <div *ngIf="appuser.AppUserEmail.invalid && (appuser.AppUserEmail.dirty || appuser.AppUserEmail.touched)"
                          class="alert alert-danger">
                          <div *ngIf="appuser.recoverPassword.errors.required"> Please Enter the Email Address.</div>
                          <div *ngIf="appuser.recoverPassword.errors.email"> Enter the valid Email Address</div>
                        </div>
                      </div>
                      <div class="form-group col-lg-12">
                        <!-- <label for="name">{{ AppQuestions }}</label> -->
                        <label>Question</label>
                        <!-- <label for=""  formControlName="AppUserquestion"> {{AppUserquestion}}</label> -->
                        <input type="text"  formControlName="AppUserquestion" id="name"  class="form-control" [attr.disabled]="true" >
                        <!-- {{ AppUserquestion.value }} -->
                      </div>
                      <div class="form-group col-lg-12">
                        <label>Answer</label>
                        <input type="text" formControlName="AppUserAnswer"   class="form-control" >
                            <div *ngIf="appuser.AppUserAnswer.invalid && (appuser.AppUserAnswer.dirty || appuser.AppUserAnswer.touched)"
                              class="alert alert-danger">
                              <div *ngIf="appuser.AppUserAnswer.errors.required"> Please fill the answer.</div>
                            </div>
                      </div>
                      <div class="form-group col-lg-12">
                        <button class="btn btn-info btn-block" type="submit">Send</button>
                      </div>
                    </div>
                  </form>
                  <div class="row">
                    <div class="col-md-12 text-center">
                      <strong>New to CareOnline?</strong> <a>Sign Up Free</a>
                    </div>
                </div>
                </div>
              </div>
            </div>
          </div>
        </div>

      </div>

      <!-- end  -->
       <!-- app user question start  -->
