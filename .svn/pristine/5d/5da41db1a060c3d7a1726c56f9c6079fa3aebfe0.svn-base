<div class="business-menu post-box-shadow" style="background-color: #fff;">
  <ul>
    <li [ngClass]="{'active': selectMenu === 'ALL'}"><a (click)="onSelect('ALL')">All</a></li>
    <li [ngClass]="{'active': selectMenu === 'USER'}"><a (click)="onSelect('USER')">People</a></li>
    <li [ngClass]="{'active': selectMenu === 'post'}"><a (click)="onSelect('post')">Post</a></li>
    <li [ngClass]="{'active': selectMenu === 'BUSINESS_PAGE'}"><a (click)="onSelect('BUSINESS_PAGE')">Business</a></li>
    <li [ngClass]="{'active': selectMenu === 'COMMUNITY_PAGE'}"><a (click)="onSelect('COMMUNITY_PAGE')">Communities</a></li>
    <li [ngClass]="{'active': selectMenu === 'people'}"><a (click)="onSelect('people')">School/College</a></li>
    <li><a (click)="onSelect('people')">Jobs</a></li>
    <li><a (click)="onSelect('people')">#Tags</a></li>
  </ul>
</div>
