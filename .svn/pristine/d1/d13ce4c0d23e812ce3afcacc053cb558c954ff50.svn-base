                        <option value=null disabled [selected]=true>Duration</option>
                        <option *ngFor="let year of years" [value]="year">{{year}}</option>
                      </select>
                    </div>
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <select type="text" formControlName="durationType" class="form-control input-sm">
                        <option value=null disabled [selected]=true>Type</option>
                        <option *ngFor="let durationType of durationTypeList" [value]="durationType.item">
                          {{durationType.item}}
                        </option>
                      </select>
                    </div>
                  </div>


                </div>

                <div class="col-sm-6 form-group">
                  <label for="duration" class="control-label required-field font-bold">Experience in Years</label>
                  <div class="row">
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <select type="text" formControlName="experienceFrom" class="form-control input-sm">
                        <option value=null disabled [selected]=true>From</option>
                        <option *ngFor="let year of years" [value]="year">{{year}}</option>
                      </select>


                      <div *ngIf="
                  (submit && jobControls.experienceFrom.invalid) ||
                  (jobControls.experienceFrom.invalid &&
                    (jobControls.experienceFrom.dirty ||
                      jobControls.experienceFrom.touched))
                " class="error-msg">
                        <div *ngIf="jobControls.experienceFrom.errors.required">
                          From Year is required
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <select type="text" formControlName="experienceTo" class="form-control input-sm">
                        <option value=null disabled [selected]=true>To</option>
                        <option *ngFor="let year of years" [value]="year">{{year}}</option>
                      </select>


                      <div *ngIf="
                  (submit && jobControls.experienceTo.invalid) ||
                  (jobControls.experienceTo.invalid &&
                    (jobControls.experienceTo.dirty ||
                      jobControls.experienceTo.touched))
                " class="error-msg">
                        <div *ngIf="jobControls.experienceTo.errors.required">
                          To Year is required
                        </div>
                      </div>

                    </div>
                  </div>
                </div>

                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <label for="targetRate" class="control-label required-field font-bold">Target Rate</label>
                      <div style="display: flex;">
                        <label for="USD" class="control-label font-bold">USD</label>
                        <input type="text" class="form-control input-sm" placeholder="Target Rate" id="targetRate"
                          formControlName="targetRate">

                      </div>
                      <div *ngIf="
                      (submit && jobControls.targetRate.invalid) ||
                      (jobControls.targetRate.invalid &&
                        (jobControls.targetRate.dirty ||
                          jobControls.targetRate.touched))
                    " class="error-msg">
                        <div *ngIf="jobControls.targetRate.errors.required">
                          This is required
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <label for="payType" class="control-label required-field font-bold">Pay Type</label>
                      <select type="text" formControlName="payType" class="form-control input-sm">
                        <option [selected]=true value=null>Choose Type</option>
                        <option *ngFor="let list of payTypeList" [value]="list">{{list}}</option>
                      </select>
                      <div *ngIf="
                      (submit && jobControls.payType.invalid) ||
                      (jobControls.payType.invalid &&
                        (jobControls.payType.dirty ||
                          jobControls.payType.touched))
                    " class="error-msg">
                        <div style="position: relative; top: 4px;" *ngIf="jobControls.payType.errors.required">
                          Pay Type is required
                        </div>
                      </div>
                    </div>
                  </div>
                </div>


                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-7" style="padding-bottom: 7px;">
                      <label for="remoteWork" class="control-label  font-bold">Remote Work</label>
                    </div>
                    <div class="col-sm-5" style="position: relative; right: 22px;">
                      <div class="row" style="    margin-top: 6px;">
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> Yes
                              <input type="radio" [value]="true" formControlName="remoteWork" name="remoteWork">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> No
                              <input type="radio" [value]="false" formControlName="remoteWork" name="remoteWork">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                </div>

                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-7" style="padding-bottom: 7px;">
                      <label for="relocationRequired" class="control-label  font-bold">Relocation Required</label>
                    </div>
                    <div class="col-sm-5" style="position: relative; right: 22px;">
                      <div class="row" style="    margin-top: 6px;">
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> Yes
                              <input type="radio" [value]="true" formControlName="relocationRequired"
                                name="relocationRequired">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> No
                              <input type="radio" [value]="false" formControlName="relocationRequired"
                                name="relocationRequired">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                </div>

                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-7" style="padding-bottom: 7px;">
                      <label for="securityClearance" class="control-label  font-bold">Security Clearance</label>
                    </div>
                    <div class="col-sm-5" style="position: relative; right: 22px;">
                      <div class="row" style="    margin-top: 6px;">
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> Yes
                              <input type="radio" [value]="true" formControlName="securityClearance"
                                name="securityClearance">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>

                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> No
                              <input type="radio" [value]="false" formControlName="securityClearance"
                                name="securityClearance">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>

                  </div>
                </div>

                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-7" style="padding-bottom: 7px;">
                      <label for="workFromHome" class="control-label  font-bold">Work From Home</label>
                    </div>
                    <div class="col-sm-5" style="position: relative; right: 22px;">
                      <div class="row" style="    margin-top: 6px;">
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> Yes
                              <input type="radio" [value]="true" formControlName="workFromHome" name="workFromHome">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                        <div class="col-sm-6" style="padding-bottom: 7px;">
                          <div class="custom-checkbox margin-top-5">
                            <label class="checkbox-label font-normal"> No
                              <input type="radio" [value]="false" formControlName="workFromHome" name="workFromHome">
                              <span title="Check Working Days" class="checkmark"></span>
                            </label>
                          </div>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

                <div class="col-sm-12">
                  <label class="control-label required-field font-bold">Job Description</label>
                  <div formControlName="jobDescription" [ngxSummernote]="config2" class="note-editor.note-frame"></div>
                  <div *ngIf="
                  (submit && jobControls.jobDescription.invalid) ||
                  (jobControls.jobDescription.invalid &&
                    (jobControls.jobDescription.dirty ||
                      jobControls.jobDescription.touched))
                " class="error-msg">
                    <div *ngIf="jobControls.jobDescription.errors.required">
                      Job Description is required
                    </div>
                  </div>

                </div>
              </div>
            </div>
          </form>
        </div>
        <div class="panel-footer" *ngIf="commonVariables.jobPostingFlag">
          <div class="shifu">
            <button class="btn btn-sm btn-default" (click)="closeJob()">Cancel</button>
            <button class="btn btn-sm btn-primary" type="submit" (click)="createJob()">Save</button>
          </div>
        </div>
      </div>
      <!-- </div> -->


      <!-- Candidate posting -->
      <div>
        <!-- [@slide-in]="animState2" -->
        <div class="panel-heading" *ngIf="commonVariables.candidatePostingFlag">
          <div class="row panel-tools">
            <div class="col-sm-2">
              <button type="button" class="close" aria-label="Close" (click)="closeCandidate()">
                <span style="font-size: 26px" aria-hidden="true">&times;</span>
              </button>
            </div>
          </div>

          <label class="control-label padding-top-0 margin-bottom-5 margin-top-5"
            style="margin-left: 8px !important">Create Candidate</label>
        </div>
        <div class="panel-body" *ngIf="commonVariables.candidatePostingFlag">
          <form [formGroup]="candidatePostForm" action="">
            <div class="app-form">
              <div class="form-group row">


                <div class="dropdox" appDnd (fileDropped)="onFileDropped($event)">
                  <input accept="application/pdf, image/jpeg, image/jpg, application/docx, application/doc," type="file"
                    #fileDropRef id="fileDropRef" (change)="fileBrowseHandler($event.target.files)" />

                  <div>
                    <h6>Drag your files here</h6>
                  </div>
                  <div>
                    <h6>or</h6>
                  </div>
                  <div>
                    <label for="fileDropRef">Click in this area</label>
                  </div>
                  <div>
                    <small>Only PDF, DOCX, DOC, and TXT file formats supported</small>
                  </div>
                </div>
                <div class="files-list" *ngIf="fileDragdrop">
                  <div class="single-file">
                    <div class="file-icon" style="width: 50px"></div>
                    <div class="info">
                      <h4 class="name">
                        {{ fileDragdrop?.name }}
                      </h4>
                      <!-- <p class="size"></p> -->
                    </div>
                  </div>
                  <!-- <button (click)="savetest()">save</button> -->

                </div>

                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <label for="firstName" class="control-label required-field font-bold">First Name</label>
                  <input type="text" class="form-control input-sm" placeholder="First Name" id="firstName"
                    formControlName="firstName">
                  <div *ngIf="
               (candidateSubmit && candidateControls.firstName.invalid) ||
               (candidateControls.firstName.invalid &&
                 (candidateControls.firstName.dirty ||
                   candidateControls.firstName.touched))
             " class="error-msg">
                    <div *ngIf="candidateControls.firstName.errors.required">
                      First Name is required
                    </div>
                  </div>
                </div>
                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <label for="lastName" class="control-label required-field font-bold">Last Name</label>
                  <input type="text" class="form-control input-sm" placeholder="Last Name" id="lastName"
                    formControlName="lastName">
                  <div *ngIf="
               (candidateSubmit && candidateControls.lastName.invalid) ||
               (candidateControls.lastName.invalid &&
                 (candidateControls.lastName.dirty ||
                   candidateControls.lastName.touched))
             " class="error-msg">
                    <div *ngIf="candidateControls.lastName.errors.required">
                      Last Name is required
                    </div>
                  </div>
                </div>
                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <label for="phone" class="control-label font-bold">Phone</label>
                  <input type="text" class="form-control input-sm" placeholder="Phone" id="phone"
                    formControlName="phone">
                  <div class="error-msg">
                    <div *ngIf="candidateControls.phone.invalid">Please enter only numerals (0-9)</div>
                  </div>

                </div>
                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <label for="clientName" class="control-label  font-bold">Email ID</label>
                  <input type="text" class="form-control input-sm" placeholder="Email ID" id="email"
                    formControlName="email">
                  <div class="error-msg">
                    <div *ngIf="candidateControls.email.invalid">
                      Enter a valid email ID
                    </div>
                  </div>
                </div>
                <div class="col-sm-6" style="padding-bottom: 7px;">

                  <div class="row">
                    <div class="col-sm-12">
                      <label for="title" class="control-label required-field font-bold">Job Title</label>
                      <ng-select [items]="candidateJobTitleList" [addTag]="true" [hideSelected]="true"
                        dropdownPosition="bottom" formControlName="jobTitle" [multiple]="false" placeholder="Job Title">
                      </ng-select>

                      <!-- <input placeholder="Job Title" class="form-control input-sm max-hight"
                  formControlName="jobTitle" trim="blur"
                  [ngClass]="{ 'is-invalid': candidateSubmit && candidateControls.jobTitle.hasError }"
                   [typeahead]="candidateJobTitleList"  [typeaheadMinLength]="0"
                  typeaheadOptionField="jobTitle"
                  autocomplete="off" [typeaheadOrderBy]="sortConfigWork"> -->

                      <div *ngIf="
                  (candidateSubmit && candidateControls.jobTitle.invalid) ||
                  (candidateControls.jobTitle.invalid &&
                    (candidateControls.jobTitle.dirty ||
                      candidateControls.jobTitle.touched))
                " class="error-msg">
                        <div *ngIf="candidateControls.jobTitle.errors.required">
                          Job Title is required
                        </div>
                      </div>

                    </div>

                    <div class="col-sm-12">
                      <label for="availableIn" class="control-label required-field font-bold">Availability</label>
                      <select class="form-control input-sm" formControlName="availableIn" type="text"
                        (change)="onChangePeriod($event.target.value)">
                        <option value="null" selected disabled>Select One</option>
                        <option *ngFor="let list of periodList" [value]="list.item">
                          {{list.item}}</option>
                      </select>
                      <div *ngIf="
                    (candidateSubmit && candidateControls.availableIn.invalid) ||
                    (candidateControls.availableIn.invalid &&
                      (candidateControls.availableIn.dirty ||
                        candidateControls.availableIn.touched))
                  " class="error-msg">
                        <div *ngIf="candidateControls.availableIn.errors.required">
                          Availablility is required
                        </div>
                      </div>
                    </div>
                  </div>
                </div>



                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-12">
                      <label for="primarySkills" class="control-label required-field font-bold">Primary Skills</label>
                      <ng-select [items]="primarySkillsList" [addTag]="true" [hideSelected]="true"
                        dropdownPosition="bottom" formControlName="primarySkills" maxSelectedItems="3" [multiple]="true"
                        placeholder="e.g. Java, Spring, Hibernate">
                      </ng-select>
                      <div *ngIf="
                (candidateSubmit && candidateControls.primarySkills.invalid) ||
                (candidateControls.primarySkills.invalid &&
                  (candidateControls.primarySkills.dirty ||
                    candidateControls.primarySkills.touched))
              " class="error-msg">
                        <div *ngIf="candidateControls.primarySkills.errors.required">
                          Primary Skills is required
                        </div>
                      </div>
                    </div>



                    <div class="col-sm-12">
                      <label for="secondarySkills" class="control-label required-field font-bold">Secondary
                        Skills</label>
                      <ng-select [items]="secondarySkillsList" [addTag]="true" [hideSelected]="true"
                        dropdownPosition="bottom" formControlName="secondarySkills" maxSelectedItems="3"
                        [multiple]="true" placeholder="e.g. Java, Spring, Hibernate">
                      </ng-select>
                      <div *ngIf="
                (candidateSubmit && candidateControls.primarySkills.invalid) ||
                (candidateControls.primarySkills.invalid &&
                  (candidateControls.primarySkills.dirty ||
                    candidateControls.primarySkills.touched))
              " class="error-msg">
                        <div *ngIf="candidateControls.primarySkills.errors.required">
                          Secondary Skills is required
                        </div>
                      </div>
                    </div>
                  </div>
                </div>



                <div class="form-group">

                  <div class="col-sm-6" style="padding-bottom: 7px;">
                    <label for="engagementType" class="control-label required-field font-bold">Available For</label>
                    <select class="form-control input-sm" formControlName="engagementType" type="text">
                      <option value="null" selected disabled>Select Your Engagement Type</option>
                      <option *ngFor="let engagementType of jobClassificationList" [value]="engagementType.item">
                        {{engagementType.item}}</option>
                    </select>

                    <div *ngIf="
         (candidateSubmit && candidateControls.engagementType.invalid) ||
         (candidateControls.engagementType.invalid &&
           (candidateControls.engagementType.dirty ||
             candidateControls.engagementType.touched))
       " class="error-msg">
                      <div *ngIf="candidateControls.engagementType.errors.required">
                        Engagement Type is required
                      </div>
                    </div>
                  </div>


                  <div class="col-sm-6" style="padding-bottom: 7px;">
                    <label for="totalExperience" class="control-label required-field font-bold">Total Experience</label>
                    <select type="text" formControlName="totalExperience" class="form-control input-sm">
                      <option value=null disabled [selected]=true>Select Years </option>
                      <option *ngFor="let totalExperience of totalExperienceList" [value]="totalExperience.item">
                        {{totalExperience.item}}</option>
                    </select>
                    <div *ngIf="
                          (candidateSubmit && candidateControls.totalExperience.invalid) ||
                          (candidateControls.totalExperience.invalid &&
                            (candidateControls.totalExperience.dirty ||
                              candidateControls.totalExperience.touched))
                        " class="error-msg">
                      <div *ngIf="candidateControls.totalExperience.errors.required">
                        Total Experience is required
                      </div>
                    </div>
                  </div>
                </div>


                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-12" style="padding-bottom: 7px;">
                      <label for="workAuthorization" class="control-label required-field font-bold">Work
                        Authorization</label>
                      <select class="form-control input-sm" formControlName="workAuthorization" type="text">
                        <option value="null" selected disabled>Select Work Authorization</option>
                        <option *ngFor="let workAuthorization of workAuthorizationList" [value]="workAuthorization.item">
                          {{workAuthorization.item}}</option>
                      </select>
                      <div *ngIf="
                            (candidateSubmit && candidateControls.workAuthorization.invalid) ||
                            (candidateControls.workAuthorization.invalid &&
                              (candidateControls.workAuthorization.dirty ||
                                candidateControls.workAuthorization.touched))
                          " class="error-msg">
                        <div *ngIf="candidateControls.workAuthorization.errors.required">
                          Work Authorization is required
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <label for="targetRate" class="control-label required-field font-bold">Target Rate</label>
                      <div style="display: flex;">
                        <label for="USD" class="control-label font-bold">USD</label>
                        <input type="text" class="form-control input-sm" placeholder="Target Rate" id="targetRate"
                          formControlName="targetRate">

                      </div>
                      <div *ngIf="
                      (candidateSubmit && candidateControls.targetRate.invalid) ||
                      (candidateControls.targetRate.invalid &&
                        (candidateControls.targetRate.dirty ||
                          candidateControls.targetRate.touched))
                    " class="error-msg">
                        <div style="position: relative; bottom: 4px;"
                          *ngIf="candidateControls.targetRate.errors.required">
                          This is required
                        </div>
                      </div>
                    </div>
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <label for="payType" class="control-label required-field font-bold">Pay Type</label>
                      <select type="text" formControlName="payType" class="form-control input-sm">
                        <option [selected]=true value=null>Select Pay Type</option>
                        <option *ngFor="let list of payTypeList" [value]="list">{{list}}</option>
                      </select>
                      <div *ngIf="
                      (candidateSubmit && candidateControls.payType.invalid) ||
                      (candidateControls.payType.invalid &&
                        (candidateControls.payType.dirty ||
                          candidateControls.payType.touched))
                    " class="error-msg">
                        <div *ngIf="candidateControls.payType.errors.required">
                          Pay Type is required
                        </div>
                      </div>
                    </div>
                  </div>
                </div>



                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <label for="location" class="control-label  font-bold">Current Location</label>
                  <div class="row form-group">
                    <div class="col-sm-6" style="padding-bottom: 7px;">
                      <select class="form-control input-sm" formControlName="country" type="text"
                        (change)="onChangeCountry($event.target.value)">
                        <option value="null" selected disabled>Country</option>
                        <option *ngFor="let country of countryList" [value]="country.countryCode">
                          {{country.countryName}}</option>
                      </select>
                    </div>
                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='US'">
                      <input type="text" placeholder="Zip Code" formControlName="zipCode" class="form-control input-sm"
                        (keyup)="onKeyZip($event, 'candidate')">
                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='CA'">
                      <select class="form-control input-sm" formControlName="state" type="text">
                        <option value="null" selected=true disabled>State</option>
                        <option *ngFor="let state of stateListCA" [value]="state.stateName">{{state.stateName}}
                        </option>
                      </select>
                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='IN'">
                      <select class="form-control input-sm" formControlName="state" type="text">
                        <option value="null" selected=true disabled>State</option>
                        <option *ngFor="let state of stateListIN" [value]="state.stateName">{{state.stateName}}
                        </option>
                      </select>
                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='AU'">
                      <select class="form-control input-sm" formControlName="state" type="text">
                        <option value="null" selected=true disabled>State</option>
                        <option *ngFor="let state of stateListAU" [value]="state.stateName">{{state.stateName}}
                        </option>
                      </select>
                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country!='AU'&&
                                                         candidatePostForm.value.country!='IN'&&
                                                         candidatePostForm.value.country!='CA'&&
                                                         candidatePostForm.value.country!='US'">
                      <input type="text" placeholder="State" formControlName="state" class="form-control input-sm">

                    </div>
                  </div>
                  <div class="row">
                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='US'">
                      <input type="text" disabled placeholder="City" formControlName="city"
                        class="form-control input-sm">

                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country!='US'">
                      <input type="text" placeholder="City" formControlName="city" class="form-control input-sm">

                    </div>

                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country!='US'">
                      <input type="text" placeholder="zipCode" formControlName="zipCode" class="form-control input-sm">

                    </div>
                    <div class="col-sm-6" *ngIf="candidatePostForm.value.country=='US'">
                      <input type="text" disabled placeholder="State" formControlName="state"
                        class="form-control input-sm">
                    </div>
                  </div>
                </div>
                <div class="col-sm-6" style="padding-bottom: 7px;">
                  <div class="row">
                    <div class="col-sm-7" style="padding-bottom: 7px;">
                      <label for="remoteWork" class="control-label  font-bold">Remote Work</label>
                    </div>
                    <div class="col-sm-5" style="position: relative; right: 22px;">
                      <div class="row" style="    margin-top: 6px;">
