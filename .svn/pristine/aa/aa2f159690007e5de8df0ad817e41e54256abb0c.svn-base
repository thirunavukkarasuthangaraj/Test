<div class="row wrapper bgcolor seachTopNav">
  <div class="col-lg-12">
    <div class="mainNav">
      <div class="navIteam">
        <!-- <span> new 2</span> -->
      </div>
      <div class="navIteam">
        <div class="input-group f-l">
          <input
            type="text"
            class="form-control input-sm"
            [formControl]="clearInput"
            name="searchadmin"
            id="searchadmin"
          />
          <span class="input-group-addon"><i class="pe-7s-search"></i></span>
          <span class="input-group-addon" (click)="clearData()"
            ><i class="pe-7s-close"></i
          ></span>
        </div>
      </div>
      <div class="navIteam connectionbtn">
        <div class="btn-group btn-group-sm" dropdown>
