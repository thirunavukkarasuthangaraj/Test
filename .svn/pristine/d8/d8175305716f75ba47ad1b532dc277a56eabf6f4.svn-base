<div class="hpanel margin-bottom-5 font-bold">
  <div class="panel-heading hbuilt wig-c">
    {{ headerTitle }}
  </div>
  <div class="panel-body">
    <div class="row" *ngIf="candidateFilter">
      <div class="col-lg-12">
        <div class="row form-group">
          <div class="col-sm-5 font-bold">
            Skills
          </div>
          <div class="col-sm-7">
            {{candidateData?.primarySkills}}
          </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Experience </div>
          <div class="col-sm-7"> {{candidateData?.experienceFrom}} <span *ngIf="candidateData?.experienceTo">to</span>
            {{candidateData?.experienceTo}} <span
              *ngIf="candidateData?.experienceFrom || candidateData?.experienceTo">Years</span> </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Location </div>
          <div class="col-sm-7 lipsis" title="{{candidateData?.city}}, {{candidateData?.state}}"> {{candidateData?.city }} <span *ngIf="candidateData?.state">,</span>
            {{candidateData?.state }} </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Rate </div>
          <div class="col-sm-7"> {{candidateData?.targetRate}} USD /hr  </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Positions </div>
          <div class="col-sm-7"> 1 </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Status </div>
          <div class="col-sm-7"> Open </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-3"> Selected </div>
          <div class="col-sm-9 vgin">
            <ng-container *ngFor="let val of dataPasstoSkillwidgets">
              <div [class.ioxlp]="dataPasstoSkillwidgets.length<=14"
                [ngClass]="{'jsdhgf': dataPasstoSkillwidgets.length>14}" *ngIf="val.isSelected" (click)="removeTag(val)">
                {{val.firstName + " " + val.lastName}}
                <span class="clsd-icn">
                  &times;
                </span>
              </div>
            </ng-container>
          </div>
        </div>
        <div class="row">
          <button class="col-sm-12 btn-primary btn btn-sm" (click)="submitJobData(dataPasstoSkillwidgets)"> Submit
          </button>
        </div>

      </div>
    </div>

    <div class="row" *ngIf="jobfilter">
      <div class="col-lg-12">
        <div class="row form-group">
          <div class="col-sm-5 font-bold">
            Skills
          </div>
          <div class="col-sm-7">
            {{jobData?.primarySkills}}
          </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Experience </div>
          <div class="col-sm-7" *ngIf="jobData?.totalExperience">
             {{jobData?.totalExperience}} Years</div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Location </div>
          <div class="col-sm-7 lipsis" title="{{jobData?.city}}, {{jobData?.state}}"> {{jobData?.city }} <span *ngIf="jobData?.state">,</span>
            {{jobData?.state }} </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Rate </div>
          <div class="col-sm-7"> {{jobData?.targetRate}} USD /hr  </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Positions </div>
          <div class="col-sm-7"> 1 </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-5 font-bold"> Status </div>
          <div class="col-sm-7"> Open </div>
        </div>
        <div class="row form-group">
          <div class="col-sm-3"> Selected </div>
          <div class="col-sm-9 vgin">
            <ng-container *ngFor="let val of dataPasstoSkillwidgets">
              <div [class.ioxlp]="dataPasstoSkillwidgets.length<=14"
                [ngClass]="{'jsdhgf': dataPasstoSkillwidgets.length>14}" *ngIf="val.isSelected" (click)="removeTag(val)">
                {{val.jobTitle}}
                <span class="clsd-icn">
                  &times;
                </span>
              </div>
            </ng-container>
          </div>
        </div>
        <div class="row">
          <button *ngIf="candidateFilter" class="btn-block btn-primary btn btn-sm" (click)="submitCandidateData(dataPasstoSkillwidgets)"> Submit
          </button>
          <button *ngIf="jobfilter" class="btn-block btn-primary btn btn-sm" (click)="requestResume(dataPasstoSkillwidgets)"> Get Resume
          </button>
        </div>
      </div>
    </div>
  </div>
</div>
