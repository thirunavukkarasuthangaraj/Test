        <section class="dir-profile-lf-work">
          <div class="hpanel">
            <div class="panel-heading hbuilt"> Primary Practice Address
            </div>
            <div class="panel-body"> 
              <table class="m-12-w1-100"  > 
                <tr>
                  <ng-container *ngIf=" data?.primaryPracticeLocation?.length!=0">
                    <td class="font-wei-600">
                      <p> {{ data?.primaryPracticeLocation.firstLineAddress }} <span *ngIf="data?.primaryPracticeLocation.firstLineAddress.length!=0">
                          ,</span>
                        {{ data?.primaryPracticeLocation.secondLineAddress }}</p>
                      <p> {{ data?.primaryPracticeLocation.city }} <span *ngIf="data?.primaryPracticeLocation.city.length!=0"> ,</span>
                        {{ data?.primaryPracticeLocation.state }} <span *ngIf="data?.primaryPracticeLocation.state.length!=0"> ,</span>
                        {{ data?.primaryPracticeLocation.postalCode}}</p>
                      <p> {{ data?.primaryPracticeLocation.country }} </p>
                      <span *ngIf="data?.primaryPracticeLocation.telephoneNumber.length!=0">
                        Phone : {{ data?.primaryPracticeLocation.telephoneNumber }}
                      </span>
                      <span *ngIf="data?.primaryPracticeLocation.faxNumber.length!=0">
                        |  Fax : {{ data?.primaryPracticeLocation.faxNumber }}
                      </span>
                    </td>
                  </ng-container>
                </tr>
               </table> 
            </div>
          </div>
        </section>


        <section
          class="dir-profile-lf-work"
          *ngIf="data?.workExperience != null"
        >
          <div class="hpanel">
            <div class="panel-heading hbuilt">Work Experience</div>
            <div class="panel-body">
              <ng-container *ngFor="let item of data?.workExperience">
                <div class="row">
                  <div class="col-md-3 col-sm-3 col-xs-12 dir-card-logo">
                    <img src="assets/images/gallery/company.png" />
                  </div>
                  <div class="col-md-9 col-sm-9 col-xs-12 dir-card-details">
                    <p class="card-details-position">
                      {{ item?.organisationName }}
                    </p>
                    <p class="card-details-name">{{ item?.title }}</p>
                    <address>
                      <span>
                        {{ item?.city }} , {{ item?.state }},
                        {{ item?.country }}
                      </span>
                      <br />
                    </address>
                    <p class="card-details-duration">
                      {{ item?.startMonth }}-{{ item?.startYear }} -
                      <span *ngIf="item.endMonth != null"
                        >{{ item?.endMonth }} {{ item?.endYear }}</span
                      >
                      <span *ngIf="item.endMonth == null"> present</span>
                    </p>
                  </div>
                </div>
                <br />
              </ng-container>
            </div>
          </div>
        </section>
        <section
          class="dir-profile-lf-education"
          *ngIf="data?.educationDetail != null"
        >
          <div class="hpanel">
            <div class="panel-heading hbuilt">Education</div>
            <div class="panel-body">
              <ng-container *ngFor="let item of data?.educationDetail">
                <div class="row m-t-5">
                  <div class="col-md-3 col-sm-3 col-xs-12 dir-card-logo">
                    <img src="assets/images/gallery/education.png" />
                  </div>
                  <div class="col-md-9 col-sm-9 col-xs-12 dir-card-details">
                    <p class="card-details-position">{{ item?.schoolName }}</p>
                    <p class="card-details-name">
                      <span> {{ item?.degree }}</span>
                      <span> & {{ item?.speciality }} </span>
                    </p>

                    <address>
                      <span> {{ item?.street }}</span> <br />
                      <span>
                        {{ item?.city }} , {{ item?.state }} ,
                        {{ item?.country }}
                      </span>
                      <br />
                    </address>

                    <p class="card-details-duration">
                      {{ item?.startMonth }}-{{ item?.startYear }} -
                      <span *ngIf="item.endMonth != null"
                        >{{ item?.endMonth }} {{ item?.endYear }}</span
                      >
                      <span *ngIf="item.endMonth == null"> Present</span>
                    </p>
                  </div>
                </div>
                <br />
              </ng-container>
            </div>
          </div>
        </section>
        <section class="dir-profile-lf-bottom">
          <div class="hpanel">
            <div class="panel-body">
              <div class="bottom-login">
                <p>Please login to connect & see full profile</p>
                <p>
                  <button class="btn primary-btn" [routerLink]="'/login'">
                    Login
                  </button>
                </p>
                <p [routerLink]="'/signUp'">New user? <span>Sign Up</span></p>
              </div>
            </div>
          </div>
        </section>
      </div>
      <div class="col-md-3 col-sm-12 col-xs-12 padding-right-0">
        <section class="dir-profile-rh-personal">
          <div class="hpanel">
            <div class="panel-heading hbuilt">Is this your Profile ?</div>
            <div class="panel-body">
              <p>
                Claim your Personal profile immediately and to connect, share
                and interact with your Collegues!
              </p>
              <button
                class="btn primary-btn"
                [routerLink]="['/signUp']"
                [queryParams]="{
                  firstName: data?.firstName,
                  lastName: data?.lastName,
                  profileId: profileId
                }"
              >
                Claim this Personal Profile
              </button>
            </div>
          </div>
        </section>
        <section class="dir-profile-lf-similar" *ngIf="similarprofile != null && similarprofile.length!=0">
          <div class="hpanel">
            <div class="panel-heading hbuilt">Similar Physicians</div>
            <div class="panel-body">
              <ng-container *ngFor="let item of similarprofile">
                <div
                  class="similar-card pointer"
                  (click)="route(item.profileId)"
                  [routerLink]="['/directory/profile/' + item.profileId]"
                >
                  <img
                    *ngIf="item?.gender == 'FEMALE'"
                    src="assets/images/female.png"
                  />
                  <img
                    *ngIf="item?.gender == 'MALE'"
                    src="assets/images/maleimg.png"
                  />
                  <div class="similar-details">
                    <p class="dr-name">{{ item?.fullName }}</p>
                    <p class="dr-other">
                      <span *ngIf="item?.primarySpecialities != null">
                        {{ item?.primarySpecialities }},
                      </span>
                      <span *ngIf="item?.primaryTaxonomyState != null">
                        {{ item?.primaryTaxonomyState }} ,</span
                      >
                      <span *ngIf="item?.primaryTaxonomyStateCode != null">
                        {{ item?.primaryTaxonomyStateCode }}
                      </span>
                    </p>
                  </div>
                </div>
              </ng-container>
            </div>
          </div>
        </section>
      </div>
    </div>
  </div>
</div>
