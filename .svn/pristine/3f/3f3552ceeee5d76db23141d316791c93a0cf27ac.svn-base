          <div class="dir-profile-details">
            <div class="profile-details">
              <div class="profile-image">
                <!-- <img src="../../../../assets/images/directory/profile.jpg"> -->
                <img
                  *ngIf="data?.gender == 'FEMALE'"
                  src="assets/images/female.png"
                />
                <img
                  *ngIf="data?.gender == 'MALE'"
                  src="assets/images/maleimg.png"
                />
              </div>

              <div class="pro-details">
                <p class="name">{{ data?.fullNameWithCredential }}</p>
                <p class="special">{{ data?.primarySpecialities }}</p>
                <p
                  *ngIf="
                    data?.primarySubSpecialities != null &&
                    data?.primarySubSpecialities != ''
                  "
                  class="area"
                >
                  {{ data?.primarySubSpecialities }}
                </p>
                <p class="address">
                  <!-- <span>  {{data?.mailingAddress.firstLineAddress }} ,</span> <br> -->
                  <span>
                    {{ data?.primaryPracticeLocation.city }},
                    {{ data?.primaryPracticeLocation.state }}</span
                  >
                  <br />
                  <span> </span> <br />
                  <!-- <span>   {{data?.mailingAddress.country }},</span> <br>
                                                   <span>  {{data?.mailingAddress.postalCode }}.</span> <br> -->
                </p>
              </div>
            </div>
            <div class="profile-btn">
              <!-- <button class="btn primary-btn"> Login to connect & see full profile </button> -->
              <button class="btn primary-btn">Book Appointment</button>
            </div>
          </div>
        </section>
        <section class="dir-profile-lf-about" *ngIf="data?.aboutMe != null && data?.aboutMe?.length != 0">
          <div class="hpanel">
            <div class="panel-heading hbuilt">About Me</div>
            <div class="panel-body">
              <p>{{ data?.aboutMe }}</p>
            </div>
          </div>
        </section>

