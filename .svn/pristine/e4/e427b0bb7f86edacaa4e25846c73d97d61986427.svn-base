<div class="container" style="margin-top: 89px">
  <div class="col-sm-9" style="padding-right: 0px">
    <div class="row" style="z-index: 1;" [class.sticktherecss]="widgetstick" #widgetsticky>
      <div class="hpanel">
        <div class="panel-body filter-hdr-pos" [class.shdwcon]="widgetstick" #widgetsticky>
          <div class="row">
            <div class="col-sm-2">
              <h5><span *ngIf="filterApplied">Select</span> Jobs</h5>
            </div>
            <div class="col-sm-6">
              <div class="app-form">
                <div class="form-group">
                  <input placeholder="Search Job Title, Job ID, Tags" type="text" class="input-sm form-control" />
                  <i class="fa-solid fa-magnifying-glass"></i>
                </div>
              </div>
            </div>
            <div class="col-sm-4 flt-ret">
              <button *ngIf="filterOn" (click)="resetFilter()" type="button" class="btn btn-sm btn-primary">
                Clear
              </button>
              <button *ngIf="filterApplied" (click)="cancel()" type="button" class="btn btn-sm btn-primary">
                Cancel
              </button>
              <button *ngIf="!filterApplied" (click)="openFilter()" type="button" class="btn btn-sm ">
                <span class="mr-5"><img src="assets/icon/filter-pe.webp" alt="" srcset="" class="wertpoi" /></span>
                <span *ngIf="totalFilters > 0">Filter({{ totalFilters }})</span>
                <span *ngIf="totalFilters == 0">Filter</span>
                <!-- <span class="mr-5"><img src="assets/icon/filter-pe.webp" alt="" srcset=""
                  class="wertpoi" /></span>Filter -->
              </button>



            </div>
          </div>
        </div>
      </div>
    </div>
    <app-jobs infiniteScroll [infiniteScrollDistance]="5" [infiniteScrollThrottle]="1500" (scrolled)="onScrollDown()"
      [applyingCandidate]="queryParamValues"  (selectedJob)="getchildData($event)" [passValues]="response" [jobsFoundStatus]="jobFoundStatus"
      style="z-index: -1;"></app-jobs>
    <div *ngIf="(stopscrollFlag || stopscrollFlag1 || stopscrollFlag2) && response.length != 0" class="row caught_up">
      <div class="col-md-12 mg-bt-9">
        <img style="max-height: 33px" src="assets/icon/caught_up.png" alt="" />
      </div>
      <div class="col-md-12">
        <p class="biase">You've reached the bottom of the page</p>
      </div>
    </div>

    <div bsModal [config]='backdropConfig' #lgModal="bs-modal" class="modal fade" tabindex="-1" role="dialog"
      aria-labelledby="dialog-sizes-name1">
      <div class="modal-dialog modal-lg">
        <div class="modal-content">
          <div class="hpanel">
            <div class="panel-heading">
              <div class="row panel-tools">
                <button type="button" class="close pull-left" aria-label="Close" (click)="lgModal.hide()">
                  <span style="
                      padding-right: 22px;
                      font-size: 26px;
                      line-height: 0px;
                    " aria-hidden="true">&times;</span>
                </button>
              </div>
              <label class="control-label padding-top-0 margin-bottom-5 margin-top-5" style="margin-left: 8px">Filter
                Jobs</label>
            </div>
            <div class="panel-body" style="height: 80vh; overflow: scroll;overflow-x: hidden;">
              <form [formGroup]="jobFilterForm" class="custom-form">
                <div class="row form-group">
                  <div class="col-sm-3">
                    <label for="">Job Source</label>
                    <div class="row">
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Check All">
                          <label class="checkbox-label font-bold" style="text-align: center">All
                            <input disabled type="checkbox" (click)="
                                chooseJobFilter(
                                  $event,
                                  'chooseAll',
                                  'jobSource'
                                )
                              " name="chooseAll" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose My Jobs">
                          <label class="checkbox-label font-bold" style="text-align: center">My Jobs
                            <input disabled    type="checkbox" (click)="
                                chooseJobFilter($event, 'My Jobs', 'jobSource')
                              " name="My Jobs" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Team Jobs">
                          <label class="checkbox-label font-bold" style="text-align: center">Team Jobs
                            <!-- formControlName="teamJobs" -->
                            <input type="checkbox" disabled
                              (click)="
                                chooseJobFilter(
                                  $event,
                                  'Team Jobs',
                                  'jobSource'
                                )
                              " name="Team Jobs" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
