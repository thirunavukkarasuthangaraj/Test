  .dropdox {
    width: 450px;
    height: 99px;
    text-align: center;
    display: grid;
    border: dashed 3px #20384b;
    margin: 0 auto;
    border-radius: 7px;

    input {
      opacity: 0;
      // position: absolute;
      // z-index: 2;
      // width: 100%;
      // height: 100%;
      // top: 0;
      // left: 0;
    }

    label {
      color: white;
      width: 131px;
      cursor: pointer;
      height: 29px;
      border-radius: 21.5px;
      background-color: #344453;
      padding: 5px 0px;
    }

    h6 {
      // font-size: 20px;
      // font-weight: 600;
      color: #38424c;
    }
  }

  .fileover {
    animation: shake 1s;
    animation-iteration-count: infinite;
  }

  .files-list {
    max-width: fit-content;
    margin-top: 1.5rem;

    .single-file {
      padding: 0.5rem;
      margin-bottom: 1rem;
      border-radius: 3px;
      background: #cbdaf1;
      .name {
        font-size: 12px;
        font-weight: 500;
        color: #353f4a;
        margin: 0;
      }

      .size {
        font-size: 12px;
        font-weight: 500;
        color: #a4a4a4;
        margin: 0;
        margin-bottom: 0.25rem;
      }

      .info {
        width: 100%;
        text-align: center;
      }
    }
  }
