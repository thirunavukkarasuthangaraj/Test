<app-langding-page-nav-bar></app-langding-page-nav-bar>
<div class=" sidebar-scroll post-page-background">

  <div class="container" style="margin-top: 2.5rem;">
    <app-business-banner (onAdd)="addTodo($event)"> </app-business-banner>
    <div class="row margin-top-5">
      <div class="container">
      <div class="col-sm-2 col-md-2 padding-left-0 padding-right-5">
        <app-navigation-header></app-navigation-header>
      </div>
     <div class="col-sm-10" style="padding: 0px !important; min-height: 120vh;">
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 " *ngIf="menu =='home'">
        <app-bussiness-post [commonemit]="passdata" [postView]="'BUSINESS'"></app-bussiness-post>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='about'">
        <app-business-profile [commonemit]="passdata"></app-business-profile>
      </div>
      <!-- padding-left-5 -->
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='follower'">
        <app-business-follower [commonemit]="passdata"></app-business-follower>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='followerRequestsReceived'">
        <app-business-followers-request [commonemit]="passdata"></app-business-followers-request>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='followerRequestsSent'">
        <app-business-followers-request-sent [commonemit]="passdata"></app-business-followers-request-sent>
      </div>
      <!-- <div class="col-sm-70 col-lg-70 col-md-70 feed-ln padding-right-0">
              <ng-container *ngIf="menu == 'community'">
              <app-business-community [commonemit]="commondata"></app-business-community>
             </ng-container>
             </div> -->
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'community'">
        <!-- <ng-container *ngIf="menu == 'community'"> -->
        <app-business-community [commonemit]="commondata"></app-business-community>
        <!-- </ng-container> -->
      </div>
      <!-- padding-left-5 -->
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='employee'">
        <app-business-employee [commonemit]="passdata"></app-business-employee>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='requestsReceived'">
        <app-business-employee-requests [commonemit]="passdata"></app-business-employee-requests>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='pageadmin'">
        <app-business-admin [commonemit]="passdata"></app-business-admin>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu =='deactivate'">
        <app-deactivate-business></app-deactivate-business>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 "  *ngIf="menu=='anayltics'">
        <app-business-analytics [commonemit]="passdata"></app-business-analytics>
      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 " *ngIf="menu=='postJobs'">

      </div>
      <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 " *ngIf="menu=='Jobs'">

      </div>
      <div class="col-sm-30 col-lg-30 col-md-30" *ngIf="menu=='home'||menu =='about'">
        <app-common-widget [page]="'BUSINESS_HOME_PAGE'" [inputData]="'BUSINESS'" [userType]=userType [viewByMember]=adminWidgets>
        </app-common-widget>
      </div>
      <div class="col-sm-30 col-lg-30 col-md-30">
        <ng-container *ngIf="menu =='followerRequestsSent'">
          <app-common-widget [page]="'BUSINESS_FOLLOWR_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>
        <ng-container *ngIf="menu =='followerRequestsReceived'">
          <app-common-widget [page]="'BUSINESS_FOLLOWR_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>
        <!-- <ng-container *ngIf="menu =='requestsReceived'">
          <app-common-widget [page]="'BUSINESS_FOLLOWR_PAGE'" [inputData]="'BUSINESS'" [userType]=userType>
          </app-common-widget>
        </ng-container> -->
        <ng-container *ngIf="menu =='follower'">
          <app-common-widget [page]="'BUSINESS_FOLLOWR_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>
        <ng-container *ngIf="menu =='employee'">
          <app-common-widget [page]="'BUSINESS_EMPLOYEE_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

        <ng-container *ngIf="menu =='requestsReceived'">
          <app-common-widget [page]="'BUSINESS_EMPLOYEE_REQ_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

        <ng-container *ngIf="menu =='community'">
          <app-common-widget [page]="'BUSINESS_COMMUNITY_PAGE'" [inputData]="'BUSINESS'" [userType]=userType [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

        <ng-container *ngIf="menu =='anayltics'">
          <app-common-widget [page]="'BUSINESS_ANALYTIC_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

        <ng-container *ngIf="menu =='pageadmin'">
          <app-common-widget [page]="'BUSINESS_ADMIN_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

        <ng-container *ngIf="menu =='deactivate'">
          <app-common-widget [page]="'BUSINESS_DEACTIVATE_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>
        <ng-container *ngIf="menu =='postJobs' || menu =='Jobs'">
          <app-common-widget [page]="'BUSINESS_JOB_PAGE'" [inputData]="'BUSINESS'" [userType]=userType  [viewByMember]=adminWidgets>
          </app-common-widget>
        </ng-container>

      </div>
     </div>
      </div>
    </div>
  </div>
</div>
