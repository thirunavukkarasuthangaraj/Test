<div class="b-g-clr ">
  <div *ngIf="passValues.length!=0;else noResult">
    <div class="hvrb" *ngFor="let items of passValues; let lastItem = last;">
      <div class="row uy-oi" [ngClass]="{'uy-oii': lastItem}">
        <div class="col-sm-3">
          <div class="row sa-as">
            <div class="col-sm-2 flxxy" [ngClass]="{'flxxy1': applyingJob}">
              <div  *ngIf="applyingJob && !items.isRequestedResume"  class="custom-checkbox lzx" title="Check this if this location is the headquarters">
                <label class="checkbox-label font-normal" style="text-align: center"  >
                  <ng-container >
                  <input   (change)="selectCheckbox()"  type="checkbox" value="{{items?.user?.userId}}" [(ngModel)]="items.isSelected"
                     name="list_name" />
                  <span class="checkmark" style="top: -5px"></span>
                </ng-container>
                </label>
              </div>
              <img class="img-circle" src={{items?.user?.photo}} onerror="this.src='assets/images/userAvatar.png';">
            </div>
            <div class="col-sm-9 sdjl" >
              <div style="font-weight: 600;">{{items?.user?.fullname}}</div>
              <p *ngIf="items.postedOn" class="font-10z"> {{items?.postedOn | date:'dd-MM-yyyy HH:mm:ss a'}} </p>
              <p *ngIf="items.enrolledOn" class="font-10z"> {{items?.enrolledOn | date:'dd-MM-yyyy HH:mm:ss a'}} </p>
            </div>
          </div>
          <div class="row pdnmj">
            <div class="flxxx">
              <div class="rcrtr-lbl" title="Vendor" *ngIf="items?.clientType=='Vendor'">V</div>
              <div class="rcrtr-lbl" title="Direct Hire" *ngIf="items?.clientType=='Direct Hire'">DH</div>
              <div class="rcrtr-lbl" title="End Client" *ngIf="items?.clientType=='End Client'">EC</div>
              <div class="rcrtr-lbl" title="Systems Integrator" *ngIf="items?.clientType=='Systems Integrator'">SI
              </div>
              <div class="rcrtr-lbl" title="Prime Vendor" *ngIf="items?.clientType=='Prime Vendor'">PV</div>
              <div class="rcrtr-lbl" title="Supplier" *ngIf="items?.clientType=='Supplier'">S</div>
              <div class="rcrtr-lbl" title="Staffing Agency" *ngIf="items?.clientType=='Staffing Agency'">SA</div>
              <div class="elpss" title="{{items.jobPostedBehalfOf}}">{{items.jobPostedBehalfOf}}</div>
            </div>
          </div>
        </div>
        <div class="col-sm-9">
          <div class="row flex" *ngIf="!flag">
            <div class="col-sm-10 jk-op">
              <div class="uy-tr">
                <div class="f-w-b">{{items.jobTitle}}</div>
                <div class="bj-io">
                  <div *ngFor="let data of items?.primarySkills">
                    <div [class.ioxlp]="data.length<=14" [ngClass]="{'jsdhgf': data.length>14}">
                      {{data}}
                    </div>
                  </div>
                </div>
              </div>
              <div *ngIf="!flag" class="pointer"> Experience : {{ items.experienceFrom}} to {{items.experienceTo }}
                Years</div>
              <div class="row uy-tr" *ngIf="!flag">
                <div title="{{items.clientName}}" *ngIf="items.showClientName"> <i class="fa fa-building-o"> </i>
                  {{items.clientName | truncate:20}} </div>
                <div *ngIf="items.showClientType"> <i class="fa fa-briefcase"> </i> {{items.clientType}} </div>
                <div> <i  *ngIf="items.city || items.state" class="fa fa-map-marker"> </i> {{items.city}} <span *ngIf="items.state">,</span> {{items.state}} </div>
                <div> <i class="fa fa-briefcase"> </i> {{items.jobClassification}} </div>
                <div> <i class="fa fa-usd"> </i> USD {{items.targetRate}} </div>
              </div>
              <div class="row uy-tr" *ngIf="!flag">
                <div> <i class="fa fa-eye cursor"> </i> {{items.viewedByCount}} Views </div>
                <div *ngIf="items?.isLiked!=null && !items?.isLiked "> <i class="fa fa-heart pointer"
                    (click)="like(items)"> </i> {{items.likesCount}} Like </div>
                <div *ngIf="items?.isLiked!=null && items?.isLiked"> <i class="fa fa-heart liked pointer bg-heart"></i>
                  {{items.likesCount}} Liked </div>
                <div> <i class="fa fa-users pointer"> </i> {{items.appliedCount}} <span *ngIf="items.appliedToday>0">
                    ({{items.appliedToday}} new) </span> </div>
                <div *ngIf="items.remoteWork"> <i class="fa fa-map-marker"> </i> Remote </div>

              </div>
              <app-flag type="JOB"  *ngIf="items.jobPostedBy!=userId" [id]="items.jobId" [flagTo]="items.jobPostedBy" [flags]="['Report Job as fake.']"></app-flag>
            </div>
            <div class="col-sm-2 uy-tr1">
              <!-- <button class="pointer btn btn-primary btn-sm ssm" (click)="open(items)"> View </button> -->
              <!-- <button class="pointer btn btn-primary btn-sm ssm" style="cursor: context-menu;"> Open </button> -->
              <div class="openop"  *ngIf="!applyingJob"> Open </div>
                <div  class="openop" *ngIf="applyingJob && items.isRequestedResume"> Applied </div>
              <div class="btn-group " *ngIf="items.jobPostedBy!=userId && (userType == 'BENCH_RECRUITER' || userType == 'JOB_SEEKER' || userType == 'FREELANCE_RECRUITER')"  dropdown>
                <a id="button-animated" dropdownToggle>
                  <img src="assets/action.png" class="img-width-dot-icon" />
                </a>
                <ul id="dropdown-animated" *dropdownMenu class="dropdown-menu" role="menu" style="right: 0; left: auto"
                  aria-labelledby="button-animated">
                  <li role="menuitem" >
                    <a (click)="apply(items)">
                      Apply This Job
                    </a>
                  </li>
                  <li role="menuitem">
                    <a (click)="open(items)">
                      View
                    </a>
                  </li>
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
      <hr *ngIf="!lastItem" style="margin-bottom: 0px!important;">
    </div>
  </div>
  <ng-template #noResult>
    <div class="wert">
      <div class="col-md-12 mg-bt-9">
        <img style=" max-height: 9rem;" src="assets/icon/nothing_found.png" alt="">
      </div>
      <!-- <div style="visibility: visible;" id="fetching" class="col-md-12 fig1">Fetching Jobs... </div>
      <div style="visibility: hidden;" id="zerocandidates" class="col-md-12 fig">Couldn't find any Jobs. </div> -->
      <div class="col-md-12 fig">{{jobsFoundStatus}} </div>
    </div>
  </ng-template>
</div>

