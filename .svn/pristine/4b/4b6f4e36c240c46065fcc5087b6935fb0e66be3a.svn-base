<div class="app-content container">
  <div class="row ">
    <div class="col-md-12">
      <div class="row hpanel">
        <div class="col-md-3" >
          <div class="panel-body menu-show" >
            <ul class="custom-panel pad">
              <li *ngFor="let item of WidgetList" class="navmenu" (click)="clickmenu(item)"
                [ngClass]="{ active: selectedmenu.menu == item.widgetDesc }" routerLinkActive="active"
                [routerLinkActiveOptions]="{exact: true}">
                <a class="navIteam">
                  <div class="nav-margin"> {{item.widgetDesc}}</div>
                 </a>
              </li>
            </ul>
          </div>
        </div>
        <div class="col-md-9">
          <app-suggestions [selectedMenu]="selectedmenu"></app-suggestions>
        </div>
      </div>
    </div>
  </div>
</div>




