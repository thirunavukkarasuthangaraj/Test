<div class="business-list-view col-md-12 padding-right-0 padding-left-0">

      <div class="business-list-view-search CommNav post-box-shadow margin-bottom-5 margin-right-5">
        <div class="row">
          <div class="col-sm-3">
            <!-- <label class=" control-label font-bold">{{businessData.businessName | uppercase}}</label> -->

            <ng-container *ngIf="createCommunity != 'SUPERADMIN'">
              <label class=" control-label font-bold" style="margin-top: 5px;">{{businessData.businessName | truncate :
                18 }}</label>
            </ng-container>

            <div *ngIf="SuperAdminData ==='true'">
              <a  (click)="routePage()"
                class="btn btn-primary btn-xs primary-btn" style="margin-top: 5px; margin-bottom: 5px;"
                #createcommunity>Create Community</a>
            </div>

            <div *ngIf="communityList.length >=1">
              {{communityList.length}} Community
            </div>
          </div>
          <div class="col-sm-6" style="padding-right: 0px">
            <input type="text" name="searchtxt" id="searchtxt" [(ngModel)]="searchText" placeholder="Search Communities"
              class="form-control input-sm">
          </div>
          <div class="col-sm-2">
            <button type="submit" class="btn primary-btn btn-xs" style="margin-top: 5px;"
              (click)="searchCommunity()">Search</button>
          </div>
        </div>
      </div>


