.searchBox {
  position: relative;
  top: 2.7px;
  border-radius: 0px;
    width: 100%;
    overflow: hidden;
    margin-left: 0;
    background-color: #ebedf0;
    display: block;
    width: 100%;
    height: 34px;
    padding: 6px 12px;
    font-size: 12px;
    line-height: 1.42857143;
    color: #555;
    border-radius: 3px;
    border: 1px solid #7a7575;
}
.searchBox:hover,
.searchBox:active,
.searchBox:focus {
  background-color: #dfe2e7c4;
}


.noti-badge {
  background: red;
  padding: 3px 6px;
  position: absolute;
  top: 5px;
  right: 5px;
}

.noti-list {
  color: black !important;
}

.noti-list:hover {
  color: white !important;
}

@media all and (max-width: 1279px) and (min-width: 1068px) {
  .app-header.after-login .navbar-form {
    padding-left: 170px;
  }
}

@media all and (max-width: 1440px) and (min-width: 1280px) {
  .app-header.after-login .navbar-form {
    padding-left: 160px;
  }
}

@media all and (max-width: 1024px) and (min-width: 992px) {
  .app-header.after-login .navbar-form {
    width: 40%;
    padding-left: 18px;
    margin-left: 80px;
  }
}

@media all and (max-width: 1200px) and (min-width: 1024px) {
  .app-header.after-login .navbar-form {
    padding-left: 50px;
    width: 40%;
    margin-left: 60px;
  }
}

