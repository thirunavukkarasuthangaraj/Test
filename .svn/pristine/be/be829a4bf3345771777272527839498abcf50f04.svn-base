<div class="hpanel" style="border: none !important; padding: 0px !important;"
style="background: white"   [class.landing-side1]="landingsidesticky1" #landingside1>
  <div class="panel-body" style="border: none !important ; padding: 0px 20px 0px !important;">
    <div class="row" >
      <div class="col-lg-12">
        <div class="profile-first">
          <div class="profile-img">
            <circle-progress [percent]="ProfileComplete?.completePercentage" [radius]="100" [outerStrokeWidth]="8"
              [outerStrokeColor]="'#34495e'" [innerStrokeColor]="'#ffffff'" [animation]="true" [animationDuration]="300" [showImage]="true"
              [showBackground]="true" [imageSrc]="ProfileComplete?.photo"
              [responsive]="true" [space]="3" data-toggle="tooltip" data-placement="top"
              title="{{ ProfileComplete?.completePercentage }}" (click)="route(ProfileComplete?.userId)" class="user-profile">
            </circle-progress>
          </div>
        </div>
        <div class="profile-info">
          <div class="profile-name ellipse curpoint" (click)="route(ProfileComplete?.userId)"
               data-toggle="tooltip" data-placement="top" title="{{ ProfileComplete?.firstName }} {{ ProfileComplete?.lastName }}">
              {{ ProfileComplete?.firstName }} {{ ProfileComplete?.lastName }}
          </div>
          <div class="profile-title ellipse" data-toggle="tooltip" data-placement="top"
               title="{{ ProfileComplete?.title }} @{{ ProfileComplete?.organisation}}">
               {{ ProfileComplete?.title }} <span *ngIf="ProfileComplete?.organisation">@</span>  {{ ProfileComplete?.organisation }}
          </div>


          <div class=" profile-title ellipse" data-toggle="tooltip" data-placement="top"
          title="{{ ProfileComplete?.city }}{{ProfileComplete?.state}}">
            {{ ProfileComplete?.city }}<span *ngIf="ProfileComplete?.city">{{ ", "}}</span>{{ ProfileComplete?.state }}
          </div>
        </div>
        <div [ngbCollapse]="collapseProfileCompletion" >
        <hr />
        <div style="
            display: flex;
            justify-content: space-evenly;
            align-items: center;
          ">
          <div class="profile-com-text" data-toggle="tooltip" data-placement="top"   title=" Profile Completion : {{ ProfileComplete?.completePercentage }}%">
            Profile Completion :  {{ ProfileComplete?.completePercentage }}%
          </div>

        </div>

        <div class="profile-com-btn" >
          <button class="btn btn-primary btn-xs" *ngIf="show" (click)="route(ProfileComplete.userId)">
            Complete Your Profile
          </button>
        </div>

      </div>
      </div>
    </div>

    <div class="panel-footer contact-footer ppd bt mt-6">
      <div class="row">
          <div class="col-md-4 col-sm-12 padding-left-0 padding-right-0 border-right"> <div class="contact-stat"><strong>{{ProfileComplete?.connectionCnt}}</strong><span style="font-size: 8px;">Connection</span></div> </div>
          <div class="col-md-4 col-sm-12 padding-left-0 padding-right-0 border-right"> <div class="contact-stat"><strong>{{ProfileComplete?.postCnt}}</strong><span style="font-size: 8px;">Post</span> </div> </div>
          <div class="col-md-4 col-sm-12 padding-left-0 padding-right-0"> <div class="contact-stat"><strong>0</strong><span style="font-size: 8px;">Likes </span> </div> </div>
      </div>
  </div>
  </div>
</div>
