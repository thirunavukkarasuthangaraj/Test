<div class="col-md-10 padding-0">

  <div class="col-md-9 padding-0">
    <div class="row margin-0">
      <div class="col-md-12 padding-left-0 padding-right-5">
        <div class="comNavBar bgCol">
          <div class="nav-iteam-1 m-t-8">
            <label for="">Member<span *ngIf="pesponseDisplay?.memberCnt>1">s </span> :<span>
                {{pesponseDisplay?.memberCnt}} </span></label>
          </div>
          <div class="nav-iteam-2">
            <div class="input-group f-l">
              <!-- <input type="text" class="form-control input-sm" placeholder="Search Member"
              aria-label="Amount (to the nearest dollar)" (keyup)="keyupdata($event)"
              (keyup.enter)="onsearch(MemberSearch)" name="searchadmin" id="searchadmin"
              [(ngModel)]="MemberSearch" />
            <span class="input-group-addon" (click)="onsearch(MemberSearch)" id="my_btn"><i
                class="pe-7s-search"></i></span> -->

              <input type="text" class="form-control input-sm" placeholder="Search Member"
                aria-label="Amount (to the nearest dollar)" name="searchadmin" id="searchadmin"
                [(ngModel)]="searchKey" />
              <span class="dummyBtn"></span>

            </div>
          </div>
          <div class="nav-iteam-3">
            <div class="btn-group" dropdown>
              <button id="button-basic" dropdownToggle type="button" class="btn btn-primary dropdown-toggle hgop"
                aria-controls="dropdown-basic">
                <i class="pe-7s-filter"></i>
              </button>
              <ul id="dropdown-basic" *dropdownMenu class="dropdown-menu" role="menu" aria-labelledby="button-basic">
              </ul>
            </div>
          </div>
          <div class="nav-iteam-4">

          </div>
          <div class="nav-iteam-5">
            <button type="submit" *ngIf="ownerFlag" class="btn primary-btn btn-sm f-l"
              style="min-width: 89px !important" (click)="addmember(basicTemplate)">
              Add Member <span class="iuiy" style="left: 0.2rem !important;"> <i class="pe-7s-add-user font-15"></i>
              </span> </button>
          </div>
        </div>
      </div>
    </div>
    <div class="margin-top-5">
      <div class="col-md-4 padding-left-0 padding-right-5" *ngFor="let data of membersData | customFilter:{userName:searchKey}:false ">
        <app-user-card [userDataInput]="data" [labelName]="'NETWORK'" (handleEvent)="handleUserCardEvent($event)"
          [config]="userCardConfig" [button]="'PRIMARY'"></app-user-card>
      </div>

      <div class="col-md-12 pad" *ngIf="membersData?.length==0">
        <app-nodata-found [title]="noDatafound"></app-nodata-found>
      </div>



    </div>
  </div>


  <div class="col-md-3 padding-0">
    <app-common-widget [page]="'NETWORK_MEMBER_PAGE'" [inputData]="'NETWORK'"></app-common-widget>
  </div>

</div>


<ng-template #basicTemplate>
  <form [formGroup]="membersForm" class="app-form form-horizontal">
    <div class="hpanel">
      <div class="panel-heading hbuilt b-r-6">
        <div class="row panel-tools">
          <button type="button" class="close pull-left" aria-label="Close" (click)="modalRef.hide()">
            <span style="padding-right: 22px;font-size: 26px;" aria-hidden="true">&times;</span>
          </button>
        </div>
        <label class="control-label padding-top-0 margin-bottom-5 "> Add Members </label>
      </div>
      <div class="panel-body">

        <div class="form-group row ">

          <div class="col-md-4" style="display: grid;">
            <label for="networkName" class=" control-label required-field font-light">Connected
              User</label>
          </div>

          <div class="col-md-8">
            <ng-select [items]="conditionList" [hideSelected]="false" [closeOnSelect]="false" dropdownPosition="bottom"
              formControlName="addconnection" bindLabel="userName" [multiple]="true" placeholder="Add Connection"
              [(ngModel)]="conditionSelectedItems">
              <ng-template ng-option-tmp let-item="item" let-item$="item$" let-index="index">
                <div class="custom-control custom-radio flex">
                  <input id="item-{{index}}" type="checkbox" [checked]="false" name="item-{{index}}"
                    [ngModelOptions]="{ standalone : true }" [ngModel]="item$.selected"
                    style="margin-right:5px; margin-top: 9px;">
                  <img *ngIf="item.photo!=null" class="img-show-hide" src="{{url+item.photo}}">
                  <img *ngIf="item.photo==null" class="img-show-hide" src="assets/images/userAvatar.png">
                  <label class="form-radio-label ml-2 custom-control-label">{{item.userName}}</label>
                </div>
              </ng-template>
            </ng-select>
          </div>
        </div>
      </div>
    </div>

    <div class="text-right">
      <button type="button" class="btn default-btn btn-sm canbtn" (click)="modalRef.hide()">Cancel</button>
      <button type="button" class="btn primary-btn btn-sm subbtn" (click)="submit()">Submit</button>
    </div>
  </form>
</ng-template>
