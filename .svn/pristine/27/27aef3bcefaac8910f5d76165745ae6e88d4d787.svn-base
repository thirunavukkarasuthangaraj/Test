        <div class="input-group f-l">
          <input type="text" class="form-control input-sm" (keyup)="searchData($event)" [formControl]="clearInput"
            name="searchadmin" id="searchadmin" />
          <span class="input-group-addon"><i class="pe-7s-search"></i></span>
          <span class="input-group-addon" (click)="clearData()"><i class="pe-7s-close"></i></span>
        </div>
      </div>
      <div class="navIteam connectionbtn">
        <div class="btn-group btn-group-sm" dropdown>
          <button id="button-animated" dropdownToggle type="button" class="btn btn-sm dropdown-toggle fbtn"
            aria-controls="dropdown-animated">
            <span class="mr-5"><img src="assets/icon/icons8-mail-filter-96.png" alt="" srcset=""
                class="filimg" /></span>Filter
          </button>
