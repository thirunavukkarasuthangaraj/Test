<div>
  <!--*ngIf="userData"-->
  <div class="row">
    <div class="col-md-12" style="z-index: 1;">
      <div class="row pcard">
        <div class="col-sm-70 col-lg-70 col-md-70" style="padding: 0rem 2.6rem;">
         <div class="row" >
          <div class="col-md-4 sCard" *ngFor="let item of Alldata | filterBy: term">
            <app-user-card [userDataInput]="item"  [button]="'PRIMARY'"> </app-user-card>
           </div>
           <div class="col-md-4 sCard slkpp" *ngFor="let item of businessPages | filterBy: term">
             <app-business-card [BusinessDataInput]="item"></app-business-card>
           </div>
           <div class="col-md-4 sCard" *ngFor="let item of CommunityData | filterBy: term" (click)="communityLink(item)">
             <app-community-card [data]="item"></app-community-card>
           </div>
         </div>
        </div>
        <div class="col-sm-30 col-lg-30 sCard ml-9">
          <div>
            <app-common-widget [page]="'NETWORK_PAGE'" [inputData]="'NETWORK'"></app-common-widget>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
