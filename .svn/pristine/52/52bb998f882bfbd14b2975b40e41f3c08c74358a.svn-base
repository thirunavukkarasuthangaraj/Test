    <div class="col-sm-12 padding-right-0 padding-left-0">
        <div class="hpanel margin-bottom-5">
            <div class="panel-heading hbuilt" style="border-bottom: 1 important;">
                <label class="control-label padding-top-0 margin-bottom-5 margin-top-5">Access to Applications</label>
            </div>
        </div>
        <div class="hpanel margin-bottom-5" *ngIf="accessGranted">
            <div class="panel-body">
                <form [formGroup]="appAccess">
                    <div class="row border-bottom" style="padding-bottom: 20px;">
                        <div class="col-sm-3">
                            <img class="img" height="50px" width="50px" src="assets/images/log.png" alt="">
                        </div>
                        <div class="col-sm-6">
                            <div class="row col-sm-12">
                                <p>CareOnline Personal Health Portal</p>
                            </div>

                            <div class="row col-sm-12">

                                <a *ngIf="!personalFlag" role="button"
                                    (click)="setPersonalAppAccess1(true)"><small>Request access</small></a>
                                <a *ngIf="personalFlag" role="button"
                                    (click)="personalHealthRevoke()"><small>Remove</small></a>

                                <div *ngIf="isModalShown" [config]='backdropConfig' (onHidden)="onHidden()" bsModal
                                    #autoShownModal="bs-modal" class="modal fade" tabindex="-1" role="dialog"
                                    aria-labelledby="dialog-auto-name">
                                    <!-- <div class="modal-dialog">
                                                    <div class="modal-content">
                                                          <div class="text-center" >
                                                            <button type="button" style="font-size: 31px;padding-right: 17px;padding-top: 7px;" class="close pull-right" aria-label="Close" (click)="hideModal()">
