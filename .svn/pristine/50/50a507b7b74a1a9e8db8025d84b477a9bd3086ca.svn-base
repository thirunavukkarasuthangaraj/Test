// Custom Editor style
.app-editor1 {
  .NgxEditor-Wrapper {
    border: 0px solid #ccc;

    .NgxEditor-MenuBar {
      .NgxEditor-MenuItem__Dropdown-Wrapper {
        .NgxEditor-MenuItem__Dropdown-Menu {
          z-index: 100;
        }
      }
    }

    .NgxEditor-Content {
      min-height: 60px;
    }

    &:focus {
      border-color: #66afe9;
      outline: 0;
      box-shadow: none;
    }
  }
}

// CUSTOM MULTI SELECT

.custom-multiselect {
  .selected-list {
    .c-btn {
      min-height: 30px;
      border-radius: 0;
      border: 1px solid #ccc;
      font-size: inherit;
      &:focus {
        border-color: #66afe9;
        outline: 0;
        box-shadow: none;
      }
    }
  }

  .dropdown-list {
    padding-top: 0;
    .arrow-up {
      display: none;
    }
    .list-area {
      border-radius: 0;
    }
  }

  .pure-checkbox input[type="checkbox"] + label:before {
    border: 1px solid #ccc;
    border-radius: 0;
  }

  .pure-checkbox input[type="checkbox"]:checked + label:before {
    background-color: #2a3e73;
    border-color: #2a3e73;
  }

  .pure-checkbox input[type="checkbox"]:checked + label:after {
    color: #fff;
  }
}



