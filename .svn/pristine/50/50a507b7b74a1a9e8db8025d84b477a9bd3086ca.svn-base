.custom-alert {
  width: 100%;
  position: fixed;
  bottom: 0px;
  background-color: #f7f7f7;
  padding: 4px 16px;
  // background-image: linear-gradient(90deg, rgba(247, 247, 247, 0.4) 1%, rgba(247, 189, 63, 0.4) 43%, rgba(255, 95, 0, 0.4) 73%, rgba(249, 7, 36, 0.4) 100%);

  .alert-model {
    display: flex;
    float: left;
    align-items: center;
    background-color: #fff;
    padding: 8px 12px 8px 8px;
    box-shadow: 0px 1px 4px 0px rgba(159, 158, 160, 0.6);
    display: none;

    i {
      padding-right: 8px;
    }

    .alert-title {
      font-weight: 500;
      padding-right: 8px;
      font-size: 14px;
    }

    .alert-desc {
      font-size: 12.5px;
    }

    &.ERROR {
      display: block;
      border-left: 4px solid #f44337;

      .alert-title {
        color: #f44337;
      }

      i {
        color: #f44337;
      }
    }

    &.SUCCESS {
      display: block;
      border-left: 4px solid #00c852;
      .alert-title {
        color: #00c852;
      }
      i {
        color: #00c852;
      }
    }
    &.INFO {
      display: block;
      border-left: 4px solid #039be6;
      .alert-title {
        color: #039be6;
      }
      i {
        color: #039be6;
      }
    }
  }
}

