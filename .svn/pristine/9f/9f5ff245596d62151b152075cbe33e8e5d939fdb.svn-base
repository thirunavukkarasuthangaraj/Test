<app-langding-page-nav-bar></app-langding-page-nav-bar>

<div class=" sidebar-scroll post-page-background">
  <div class="container" style="margin-top: 2.5rem;">
    <app-community-banner (onAddcommunity)="addTodo($event)">
    </app-community-banner>
    <div class="row margin-top-5">
      <div class="container">
        <div class="col-sm-2 padding-left-0 padding-right-5">
          <app-navheadercommunity></app-navheadercommunity>
        </div>
        <div class="col-sm-10" style="padding: 0px !important;">
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'communityhome'">
            <app-bussiness-post [commonemit]="passdata" [postView]="'COMMUNITY'"></app-bussiness-post>
          </div>
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0  " *ngIf="menu == 'communityabout'">
            <app-community-profile [commonemit]="passdata"></app-community-profile>
          </div>
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'communitypageadmin'">
            <app-community-pageadmin [commonemit]="passdata"></app-community-pageadmin>
          </div>
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'employee'">
            <app-business-employee [commonemit]="passdata"></app-business-employee>
          </div>
  
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'managepages'">
            <app-managepage [commonemit]="passdata"></app-managepage>
          </div>
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 " *ngIf="menu == 'memberspage'">
            <app-members [commonemit]="passdata"></app-members>
          </div>
  
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'request'">
            <app-request [commonemit]="passdata"></app-request>
          </div>
  
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0 " *ngIf="menu == 'Invite'">
            <app-invitesend [commonemit]="passdata"></app-invitesend>
          </div>
  
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0" *ngIf="menu == 'anayltics'">
            <app-business-analytics [commonemit]="passdata"></app-business-analytics>
          </div>
  
          <div class="col-sm-70 col-lg-70 col-md-70  padding-right-0 padding-left-0"
            *ngIf="menu == 'deactivate'">
            <app-deactivate-community></app-deactivate-community>
          </div>
  
          <div class="col-sm-30 col-lg-30 col-md-30  padding-right-0 padding-left-0 "
            *ngIf="menu == 'communityhome' || menu == 'communityabout'">
            <app-common-widget [page]="'COMMUNITY_HOME_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
            </app-common-widget>
          </div>
  
          <div class="col-sm-30 col-lg-30 col-md-30   padding-right-0 padding-left-0 ">
            <ng-container *ngIf="menu == 'Invite'">
              <app-common-widget [page]="'COMMUNITY_INVITE_SENT_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
              </app-common-widget>
            </ng-container>
  
            <ng-container *ngIf="menu == 'request'">
              <app-common-widget [page]="'COMMUNITY_REQUEST_RECEIVED_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
              </app-common-widget>
            </ng-container>
  
            <ng-container *ngIf="menu == 'communitypageadmin'">
              <app-common-widget [page]="'COMMUNITY_ADMIN_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
              </app-common-widget>
            </ng-container>
  
            <ng-container *ngIf="menu == 'memberspage'">
              <app-common-widget [page]="'COMMUNITY_MEMBER_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
              </app-common-widget>
            </ng-container>
  
            <ng-container *ngIf="menu == 'deactivate'">
              <app-common-widget [page]="'COMMUNITY_DEACTIVATE_PAGE'" [inputData]="'COMMUNITY'" [userType]=userType [viewByMember]=adminWidgets>
              </app-common-widget>
            </ng-container>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
