<aside *ngIf="localUser">
  <div class="business-menu">
    <div>
      <ul class="nav" id="side-menu">
        <ng-container>
          <div>
            <div class="ty">
              <div class="list-item" [ngClass]="{'active':pageName == 'personalProfile'}">
                <div class="panel-tools" (click)="dropFlag = !dropFlag" [attr.aria-expanded]="dropFlag">
                  <i *ngIf="!dropFlag" class="fa fa-chevron-down"></i>
                  <i *ngIf="dropFlag" class="fa fa-chevron-up"></i>
                </div>
                <div class="cursor-ponit" [routerLink]="['/personalProfile']" [queryParams]="{userId: data.userId}"
                  (click)="menuClick('personalProfile')" routerLinkActive="active">
                  <span style="margin-right: 1.5rem;"><i class="fas fa-user fa-lg"></i></span>
                  <span class="elps"> Profile</span>
                </div>
              </div>
            </div>
            <div class="custom-panel" [collapse]="!dropFlag" [isAnimated]="true">
              <ul id="grouplist" class="list-group  ">
                <ng-container>
                  <li class="list-group-item cursor-point br-none" (click)="menuClick('workExperience')"
                    [ngClass]="{'active' : pageName === 'workExperience'}" routerLinkActive="router-link-active"> <span
                      class="elps" style="margin-left: 4.6rem;">Work Experience</span>
                  </li>
                  <li class="list-group-item cursor-point br-none" [ngClass]="{'active' : pageName === 'education'}"
                    (click)="menuClick('education')" routerLinkActive="router-link-active"> <span class="elps"
                      style="margin-left: 4.6rem;">Education</span>
                  </li>
                  <li class="list-group-item cursor-point br-none" [ngClass]="{'active' : pageName === 'certification'}"
                    (click)="menuClick('certification')" routerLinkActive="router-link-active"> <span class="elps"
                      style="margin-left: 4.6rem;">Certification</span>
                  </li>
                  <li class="list-group-item cursor-point br-none" (click)="menuClick('socialInfluence')"
                    [ngClass]="{'active' : pageName === 'socialInfluence'}" routerLinkActive="router-link-active"> <span
                      class="elps" style="margin-left: 4.6rem;">Social Influence</span>
                  </li>
                </ng-container>
              </ul>
            </div>
          </div>


          <li class="list-item" [ngClass]="{'active' : pageName === 'settings'}" (click)="menuClick('settings')" style="cursor: pointer;
       border-radius: 0px; border: 0px;" routerLinkActive="router-link-active">
            <span style="margin-right: 1.2rem;"><i class="fas fa-cog fa-lg"></i></span>
            <span class="elps"> Security Settings</span>
          </li>
          <li class="list-item" [ngClass]="{'active' : pageName === 'appAccess'}" (click)="menuClick('appAccess')"
            style="cursor: pointer;
      border-radius: 0px; border: 0px;" routerLinkActive="router-link-active">
            <span style="margin-right: 1.2rem;"><i class="far fa-check-circle fa-lg"></i></span>
            <span class="elps"> Application Access </span>
          </li>
          <!-- <li class="list-item" [ngClass]="{'active' : pageName === 'social'}" (click)="menuClick('social')" style="cursor: pointer;
      border-radius: 0px; border: 0px;" routerLinkActive="router-link-active">
            <span style="margin-right: 1.6rem;"><i class="fas fa-project-diagram"></i></span>
            <span class="elps">Social Media Access</span>
          </li> -->
          <li class="list-item" (click)="menuClick('communities')" [ngClass]="{'active' : pageName === 'communities'}"
            style="cursor: pointer;
                          border-radius: 0px; border: 0px;" routerLinkActive="router-link-active">
            <span style="margin-right: 1.2rem;"><i class="fas fa-users fa-lg"></i></span>
            <span class="elps">Communities</span>
          </li>
        </ng-container>
      </ul>
    </div>
  </div>
</aside>