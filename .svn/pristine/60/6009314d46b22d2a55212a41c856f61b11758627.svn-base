<div class="col-md-10 padding-0"> 
<div class="col-md-9 padding-0">
<!-- postContained wd -->
  <!-- <div class="row"> -->
    <div class="col-sm-12 padding-left-0 padding-right-5">
      <div class="hpanel">
        <div class="panel-heading hbuilt">
          <div class="panel-tools">
            <div class="pull-right btn-group">
              <div class="col-sm-2" title="Edit about company" *ngIf="ownerFlag"
                (click)="editBasic(basicTemplate)">
                <button class="edit-pencil">
                  <span>
                    <i class="fas fa-pencil-alt editBG icon-over"></i>
                  </span>
                </button>
              </div>
            </div>
          </div>
          <label class="control-label padding-top-0 margin-bottom-5 margin-top-5">About</label>
        </div>

        <div class="panel-body" style="word-wrap: break-word">
          <div class="row form-group">
            <div class="col-sm-3">Network Name </div>
            <div class="col-sm-9"> {{ pesponseDisplay?.networkName }}</div>
          </div>
          <div class="row">
            <div class="col-sm-3">Description </div>
            <div class="col-sm-9">
              <div *ngIf="pesponseDisplay?.description;else erg"
                [ngxSummernoteView]="pesponseDisplay?.description"></div>
              <ng-template #erg>Not provided</ng-template>
            </div>
          </div>
        </div>
      </div>
    </div>
  <!-- </div> -->
</div>
<div class="col-md-3 padding-0">
  <app-common-widget [page]="'NETWORK_ABOUT_PAGE'" [inputData]="'NETWORK'"></app-common-widget>
</div>
</div>

<ng-template #basicTemplate>
  <form [formGroup]="networkModalForm" class="app-form form-horizontal">
    <div class="hpanel">
      <div class="panel-heading hbuilt b-r-6">
        <div class="row panel-tools">
          <button type="button" class="close pull-left" aria-label="Close" (click)="close()">
            <span style="padding-right: 22px;font-size: 26px;" aria-hidden="true">&times;</span>
          </button>
        </div>
        <label class="control-label padding-top-0 margin-bottom-5 ">Network Details</label>
      </div>
      <div class="panel-body">
        <div class="app-form">
          <div class="form-group row ">
            <div class="col-md-4" style="display: grid;">
              <label for="networkName" class=" control-label required-field font-light">Network Name</label>
            </div>

            <div class="col-md-7">
              <input type="text" class="form-control input-sm" placeholder="Network Name" formControlName="networkName"
                trim="blur" />
              <div *ngIf="submit && f.networkName.invalid || f.networkName.invalid &&
                (f.networkName.dirty || f.networkName.touched)" class="error-msg">

                <!-- <div class="red" *ngIf="f.networkName.errors.pattern">Please avoid unwanted spaces
                 </div> -->
              </div>
            </div>
          </div>



          <div class="form-group row ">
            <div class="col-md-4" style="display: grid;">
              <label for="networkName" class=" control-label   font-light"> Network Logo </label>
            </div>

            <div class="col-md-7">
              <input type="file" class="form-control input-sm" accept="image/jpg, image/jpeg, image/png" #myFileInput
                (change)="fileChangeEventlogo($event, template)" (click)="myFileInput.value = null" />

            </div>
          </div>

          <div class="form-group row ">
            <div class="col-md-4" style="display: grid;">
              <label for="networkName" class=" control-label   font-light"> Network Banner Image</label>
            </div>

            <div class="col-md-7">
              <input type="file" class="form-control input-sm" accept="image/jpg, image/jpeg, image/png"
                #myFileInputbanner (change)="fileChangeEventBanner($event, templatebanner)"
                (click)="myFileInputbanner.value = null" />

            </div>
          </div>


          <div class="form-group row ">
            <div class="col-md-4" style="display: grid;">
              <label for="networkName" class=" control-label   font-light">Description</label>
            </div>

            <div class="col-md-7">
              <!-- <input type="textarea" class="form-control input-sm" placeholder="Description"
                formControlName="Description" rows="4"/> -->

              <textarea class="form-control input-sm" placeholder="Description" formControlName="description"
                [ngxSummernote]="config" name="" id="" cols="30" rows="4"></textarea>

            </div>
          </div>
        </div>
      </div>
    </div>

    <div class="text-right">
      <button type="button" class="btn default-btn btn-sm canbtn" (click)="close()">Cancel</button>
      <button type="button" class="btn primary-btn btn-sm subbtn" [disabled]="!networkModalForm.valid"
        (click)="submit()">Submit</button>
    </div>
  </form>
</ng-template>


<ng-template #template>


  <div class="modal-content">
    <div style="padding: 14px 14px !important;">
      <label>Network Logo</label>
      <button type="button" class="close pull-right" aria-label="Close" (click)="closePhoto()">
        <span style="font-size: 2.5rem;" aria-hidden="true">&times;</span>
      </button>
    </div>
    <hr style="margin: 0px !important; padding: 0px !important">
    <div class="modal-body">
      <image-cropper [imageChangedEvent]="imageChangedEvent" [maintainAspectRatio]="true" format="jpeg"
        (imageCropped)="imageCropped($event)"></image-cropper>
    </div>

    <div class="modal-footer" style="border-radius: 5px;">
      <button class="btn primary-btn btn-sm" (click)="imageLoaded()">Apply</button>
    </div>
  </div>
  <!-- <button type="button" style="font-size: 31px; padding-right: 17px; padding-top: 7px" class="close pull-right"
    aria-label="Close" (click)="closePhoto()">
    <span aria-hidden="true">&times;</span>
  </button>
  <h5 style="margin: 10px 10px 10px 10px; padding-top: 10px">Network Logo </h5>
  <hr /> -->
</ng-template>



<ng-template #templatebanner>
  <!-- <button type="button" style="font-size: 31px; padding-right: 17px; padding-top: 7px" class="close pull-right"
    aria-label="Close" (click)="closePhoto()">
    <span aria-hidden="true">&times;</span>
  </button>
  <h5 style="margin: 10px 10px 10px 10px; padding-top: 10px"> Network Banner Image </h5>
  <hr /> -->

  <div class="modal-content">
    <div style="padding: 14px 14px !important;">
      <label>Network Banner</label>
      <button type="button" class="close pull-right" aria-label="Close" (click)="closePhoto()">
        <span style="font-size: 2.5rem;" aria-hidden="true">&times;</span>
      </button>
    </div>
    <hr style="margin: 0px !important; padding: 0px !important">
    <div class="modal-body">
      <image-cropper [imageChangedEvent]="imageChangedEvent" [maintainAspectRatio]="true" [aspectRatio]="4.5 / 1"
        format="jpeg" (imageCropped)="imageCroppedBanner($event)"></image-cropper>
    </div>

    <div class="modal-footer" style="border-radius: 5px;">
      <button class="btn primary-btn btn-sm" (click)="imageLoadedBanner()">Apply</button>
    </div>
  </div>

</ng-template>