<div class="directory-profile">
  <div class="container">
    <div class="row">
      <div class="col-md-9 col-sm-12 col-xs-12 padding-right-0">
        <section class="dir-profile-lf-personal">
          <div class="dir-banner">
            <!-- <img class="banner-img" style="background-image: url('../../../../assets/images/female.png');"> -->
            <img class="banner-img" style="background: #9aa8db" />
          </div>

          <div class="dir-profile-details">
            <div class="profile-details">
              <div class="profile-image">
                <img [src]="img.src" *ngIf="data?.businessLogo != null" />
                <img   style="background: url(assets/images/log.png) top right /32px 31px no-repeat,
                                          url(assets/images/gallery/company.png) center/94px 87px no-repeat;"
                  *ngIf="
                    data?.businessLogo == null ||
                    data?.businessLogo == undefined ||
                    data?.businessLogo == ''
                  "
                />
              </div>
            </div>

            <div class="pro-details m-r-35 fs-16">
              <p class="name fs-16 bold txt-overflow">
                {{ data?.organizationName }}
              </p>
              <p
                class="special fs-16 font-600 txt-overflow"
                *ngIf="data?.primaryHCPName == null"
              >
                {{ data?.primaryHCPType }}
              </p>
              <p
                class="special fs-16 font-600 txt-overflow"
                *ngIf="data?.primaryHCPName != null"
              >
                {{ data?.primaryHCPName }}
              </p>
              <p class="address fs-14">
                <span>
                  {{ data?.primaryPracticeLocation.city }} ,
                  {{ data?.primaryPracticeLocation.state }}</span
                >
                <br />
              </p>
            </div>

            <div class="profile-btn text-end">
              <div class="mar-btm-10">
                <button
                  (click)="redirectTo('login')"
                  class="btn-primary btn btn-xs"
                >
                  Login to connect & see full profile
                </button>
              </div>

              <div>
                <button class="btn primary-btn">Book Appointment</button>
              </div>
            </div>
          </div>
        </section>
        <section class="dir-profile-lf-about" *ngIf="data?.aboutCompany != null && data?.aboutCompany?.length != 0">
          <div class="hpanel">
            <div class="panel-heading hbuilt">About Us</div>
            <div class="panel-body">
              <!-- <p>{{ data?.aboutCompany }}</p> -->
              <div [ngxSummernoteView]="data?.aboutCompany"></div>
            </div>
          </div>
        </section>

        <section class="dir-profile-lf-work">
          <div class="hpanel">
            <div class="panel-heading hbuilt">
              Authorized Official Information
            </div>
            <div class="panel-body">
              <table style="width: 100%">
                <tr class="td-high-26">
                  <th style="width: 128px">Name</th>
                  <td>: {{ data?.authOfficialFullNameWithCredential }}</td>
                </tr>
                <tr class="td-high-26">
                  <th style="width: 128px">Title</th>
                  <td>: {{ data?.authorizedOfficialTitle }}</td>
                </tr>
                <tr class="td-high-26">
                  <th style="width: 128px">Phone</th>
                  <td>: {{ data?.authorizedOfficialTelephoneNumber }}</td>
                </tr>
              </table>
              <br />
            </div>
          </div>
        </section>

