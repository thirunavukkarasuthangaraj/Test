.sticky{
    position: fixed;
    // top: 0;
    overflow: hidden;
    z-index: 10;
    margin-top: 32px;
  }

  .nav{
    
    .list-item{
    position: relative;
    display: block;
    border-radius: 0px;
    margin-bottom: -1px;
    background-color: #fff !important;
    // border-bottom: 2px solid lavender!important;
    color: #6a6c6f;
    font-weight: 600;
    padding: 15px 20px;
    }
  .list-item:hover,
  .list-item:focus,
 
  .list-item.active:focus {
    // position: relative;
    // display: block;
    // border-radius: 0px;
    // margin-bottom: -1px;
    outline: none;
    background: #f1f3f6 !important;
    cursor: pointer;
    color: black;
    // font-weight: 700;
    // padding: 15px 20px;
    // border-bottom: 2px solid lavender !important;
    // border: none !important;
  }

  .list-item.active,
  .list-item.active:hover{
    background-color: #c1daffbd !important;
    color: black;
  }

  
  }

  // ul{
  //   li:not(:last-child){
  //     border-bottom: 2px solid lavender!important;
  //   }
  // }

  #side-menu li{
    width: 100%;
    display: flex !important;
    border-radius: 0px;
    justify-content: space-between;
  }

  #side-menu li:active{
    outline: none !important;
    background: #c1daffbd !important;
    color: black !important;
  }

  ul {
    li {
      list-style: none;
      .list-item:active {
        outline: none;
        background: #c1daffbd;
        color: black;
      }
    }
  }

  .cursor-point {
    cursor: pointer;
  }

  .ty:hover {
    background-color: #f1f3f6;
    color: black;
  }
  
  .ty:active{
    outline: none;
    background: #c1daffbd !important;
  }

  #side-menu li{
    // display: block !important;
    // border: none !important;
    padding-right: 0px;
  }

  .elps {
    text-overflow: ellipsis;
    width: 90%;
    overflow: hidden;
    white-space: nowrap;
    font-weight: 500;
  }