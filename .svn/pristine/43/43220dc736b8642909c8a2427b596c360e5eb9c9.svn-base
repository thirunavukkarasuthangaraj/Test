<div class="container business-banner-top post-box-shadow" style="margin-top: 2.5rem;">
  <div class="business-banner">
    <div class="banner-shadow"></div>
    <img class="banner-img" *ngIf="response?.bannerImage==null && response?.bannerImage==undefined"
      [ngStyle]="{ 'background-image': 'url(assets/images/networkbanner.png)' }" />
    <img class="banner-img" *ngIf="response?.bannerImage!=null && response?.bannerImage!=undefined"
      [ngStyle]="{ 'background-image': 'url( '+url+response?.bannerImage+')' }" />

    <div class="banner-overlay">
      <div class="banner-edit" *ngIf="ownerFlag">
        <input type="file" accept="image/jpg, image/jpeg, image/png" #myFileInput
          (change)="fileChangeEvent($event, template)" (click)="myFileInput.value = null" />

        <div class="image-upload"></div>
        <i class="fa fa-camera" aria-hidden="true"></i>
      </div>
      <div class="banner-profile-image">
        <img *ngIf="response?.logo==null && response?.logo==undefined" src="assets/icon/icons8-share-400.png" alt=""
          srcset="" />
        <img *ngIf="response?.logo!=null && response?.logo!=undefined" src="{{url+response?.logo}}" alt="" srcset="" />
      </div>
    </div>
  </div>

  <div class="row business-details">

    <div class="col-md-8">
         <div class="qwc">
          <strong class="bus-hov business-name"  [routerLink]="['/landingPage/network']">Network</strong>
        <p class="business-name break-word ellipse1" data-toggle="tooltip" data-placement="top"
        title="{{response?.networkName}}"> 
          <strong>:</strong>
          {{response?.networkName }} 
           <span *ngIf="response?.memberCnt!=0 && response?.networkName" class="opi">|</span>
           <span class="lkio" (click)="menuclick('members')"><span title="{{response?.memberCnt}}" *ngIf="response?.memberCnt">
            {{response?.memberCnt}} Member</span> <span *ngIf="response?.memberCnt>1">s </span></span>
          </p> 
        </div>
      </div>



      <!-- <div class="flexmain" style="display: flex; justify-content: flex-start; margin-left: 14px;">
