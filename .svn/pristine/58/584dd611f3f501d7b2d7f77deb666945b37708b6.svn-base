<section class="padding-left-20 padding-right-20 directory-title">
  <h3>Search by City</h3>
</section>

<section class="directory-list-section">
  <div class="container">
    <div class="row">
      <ul class="col-list-4 m-b-50">
        <li
          *ngFor="
            let d of cityData
              | paginate: { itemsPerPage: 40, currentPage: page }
          "
        >
          <!-- <a class="mover" routerLink='/directory/hcp/country/{{countryCode}}/state/{{d.stateCode}}/city/{{d.cityName}}/professional'>{{d.cityName}}</a> -->
          <a class="mover" (click)="cityclick(d)">{{ d.cityName }}</a>
        </li>
      </ul>
      <pagination-controls
        class="center-page-align"
        (pageChange)="page = $event"
      ></pagination-controls>
    </div>
  </div>
</section>

<section class="directory-search-footer">
  <div class="container">
    <div class="search-by-content">
      <span>Search by </span>
      <ul>
        <li><a>Healthcare Professionals </a></li>
        <li><a>Healthcare Organisation</a></li>
        <li><a>Communities </a></li>
      </ul>
    </div>
  </div>
</section>
