<div class="container" style="margin-top: 89px;">
  <div class="col-sm-9" style="padding-right: 0px;" infiniteScroll [infiniteScrollDistance]="5"
    [infiniteScrollThrottle]="1500" (scrolled)="onScrollDown()">

    <div class="row" [class.sticktherecss]="widgetstick" #widgetsticky style="z-index: 1;">
      <div class="hpanel">
        <div class="panel-body filter-hdr-pos" [class.shdwcon]="widgetstick" #widgetsticky>
          <div class="row">
            <div class="col-sm-3">
              <h5>{{filterdata.filterTitle}} Candidates</h5>
            </div>
            <div class="col-sm-5">
              <div class="app-form">
                <div class="form-group">
                  <input placeholder="Search Candidates, Candidates Title, Candidate ID, Tags" type="text"
                    class="input-sm form-control" />
                  <i class="fa-solid fa-magnifying-glass"></i>
                </div>
              </div>
            </div>
            <div class="col-sm-4 flt-ret">
              <button *ngIf="filterApplied" (click)="cancelApplication()" type="button" class="btn btn-sm  btn-primary">
                Cancel
              </button>
              <button *ngIf="filterOn && totalFilters>0" (click)="cancelFilterValues()" type="button" class="btn btn-sm  btn-primary">
                Clear
              </button>
              <button *ngIf="!filterApplied" (click)="openFilter()" type="button" class="btn btn-sm">
                <span class="mr-5"><img src="assets/icon/filter-pe.webp" alt="" srcset="" class="wertpoi" /></span>
                <span *ngIf="totalFilters > 0">Filter({{ totalFilters }})</span>
                <span *ngIf="totalFilters == 0">Filter</span>
              </button>
            </div>
          </div>
        </div>
      </div>
    </div>

    <div bsModal [config]='backdropConfig' #lgModal="bs-modal" class="modal fade" tabindex="-1" role="dialog"
      aria-labelledby="dialog-sizes-name1">
      <div class="modal-dialog modal-lg">
        <div class="modal-content">
          <div class="hpanel">
            <div class="panel-heading">
              <div class="row panel-tools">
                <button type="button" class="close pull-left" aria-label="Close" (click)="lgModal.hide()">
                  <span style="
                  padding-right: 22px;
                  font-size: 26px;
                  line-height: 0px;
                " aria-hidden="true">&times;</span>
                </button>
              </div>
              <label class="control-label padding-top-0 margin-bottom-5 margin-top-5" style="margin-left: 8px">Filter
                Candidates</label>
            </div>
            <div class="panel-body" style="height: 80vh; overflow: scroll;overflow-x: hidden;">
              <form [formGroup]="candidateFilterForm" class="custom-form">
                <div class="row form-group">
                  <div class="col-sm-3">
                    <label for="">Candidate Source</label>
                    <div class="row">
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Check All">
                          <label class="checkbox-label font-bold" style="text-align: center">All
                            <input disabled type="checkbox" disabled (click)="
                            chooseJobFilter($event, 'chooseAll', 'candidateSource')
                          " name="chooseAll" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose My Candidates">
                          <label class="checkbox-label font-bold" style="text-align: center">My Candidates
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter($event, 'My Candidates', 'candidateSource')
                          " name="My Candidates" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Team Candidates">
                          <label class="checkbox-label font-bold" style="text-align: center">Team Candidates
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter($event, 'Team Candidates', 'candidateSource')
                          " name="Team Candidates" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Bench Sales Network">
                          <label class="checkbox-label font-bold" style="text-align: center">Bench Sales Network
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'Bench Sales Network',
                              'candidateSource'
                            )
                          " name="Bench Sales Network" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Featured Candidates">
                          <label class="checkbox-label font-bold" style="text-align: center">Featured Candidates
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'Featured Candidates',
                              'candidateSource'
                            )
                          " name="Featured Candidates" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Suggested Candidates">
                          <label class="checkbox-label font-bold" style="text-align: center">Suggested Candidates
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'Suggested Candidates',
                              'candidateSource'
                            )
                          " name="Suggested Candidates" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Other Candidates">
                          <label class="checkbox-label font-bold" style="text-align: center">Other Candidates
                            <input type="checkbox" disabled (click)="
                            chooseJobFilter($event, 'Other', 'candidateSource')
                          " name="Other" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-3">
                    <label for="">Available For</label>
                    <div class="row">
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="All">
                          <label class="checkbox-label font-bold" style="text-align: center">All
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter($event, 'chooseAll', 'engagementType')
                          " name="chooseAll" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Corp to Corp">
                          <label class="checkbox-label font-bold" style="text-align: center">Corp to Corp
                            <input type="checkbox" [ngModelOptions]="{standalone: true}" [(ngModel)]="CorpToCorp"
                              (click)="
                            chooseJobFilter($event, 'Corp To Corp', 'engagementType')
                          " name="Corp To Corp" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose W2 - Contract">
                          <label class="checkbox-label font-bold" style="text-align: center">W2 - Contract
                            <input type="checkbox" [ngModelOptions]="{standalone: true}" [(ngModel)]="W2Contract"
                              (click)="
                            chooseJobFilter($event, 'W2 - Contract', 'engagementType')
                          " name="W2 - Contract" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose W2 - Full Time">
                          <label class="checkbox-label font-bold" style="text-align: center">W2 - Full Time
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="W2FullTime" type="checkbox"
                              (click)="
                            chooseJobFilter($event, 'W2 - Full Time', 'engagementType')
                          " name="W2 - Full Time" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Part Time">
                          <label class="checkbox-label font-bold" style="text-align: center">Part Time
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="PartTime" type="checkbox" (click)="
                            chooseJobFilter($event, 'Part Time', 'engagementType')
                          " name="Part Time" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Freelance">
                          <label class="checkbox-label font-bold" style="text-align: center">Freelance
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="Freelance" type="checkbox"
                              (click)="
                            chooseJobFilter($event, 'Freelance', 'engagementType')
                          " name="Freelance" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Internship">
                          <label class="checkbox-label font-bold" style="text-align: center">Internship
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="Internship" type="checkbox"
                              (click)="
                            chooseJobFilter($event, 'Internship', 'engagementType')
                          " name="Internship" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Other">
                          <label class="checkbox-label font-bold" style="text-align: center">Other
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="W2Contract" disabled
                              type="checkbox" (click)="chooseJobFilter($event, 'Other', 'engagementType')"
                              name="Other" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-3">
                    <label for="">Status</label>
                    <div class="row">
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Check All">
                          <label class="checkbox-label font-bold" style="text-align: center">All
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter($event, 'chooseAll', 'status')
                          " name="chooseAll" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Available">
                          <label class="checkbox-label font-bold" style="text-align: center">Available
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Available', 'status')"
                              name="Available" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Shortlisted">
                          <label class="checkbox-label font-bold" style="text-align: center">Shortlisted
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Shortlisted', 'status')"
                              name="Shortlisted" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Interviews">
                          <label class="checkbox-label font-bold" style="text-align: center">Interviews
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Interviews', 'status')"
                              name="Interviews" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Offers">
                          <label class="checkbox-label font-bold" style="text-align: center">Offers
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Offers', 'status')"
                              name="Offers" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Onboarded">
                          <label class="checkbox-label font-bold" style="text-align: center">Onboarded
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Onboarded', 'status')"
                              name="Onboarded" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose On Contract">
                          <label class="checkbox-label font-bold" style="text-align: center">On Contract
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'On Contract', 'status')"
                              name="On Contract" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Featured">
                          <label class="checkbox-label font-bold" style="text-align: center">Featured
                            <input disabled type="checkbox" (click)="chooseJobFilter($event, 'Featured', 'status')"
                              name="Featured" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>

                  <div class="col-sm-3">
                    <div class="row">
                      <div class="col-sm-12">
                        <label for="">Work Authorization</label>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="All">
                          <label class="checkbox-label font-bold" style="text-align: center">All
                            <input disabled type="checkbox" (click)="
                            chooseJobFilter($event, 'chooseAll', 'workAuthorization')
                          " name="chooseAll" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="E3 Visa">
                          <label class="checkbox-label font-bold" style="text-align: center">E3 Visa
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="E3Visa" type="checkbox" (click)="
                            chooseJobFilter($event, 'E3 Visa', 'workAuthorization')
                          " name="E3 Visa" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose EAD - CPT">
                          <label class="checkbox-label font-bold" style="text-align: center">EAD - CPT
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="EadCpt" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'EAD - CPT',
                              'workAuthorization'
                            )
                          " name="EAD - CPT" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose EAD - GC">
                          <label class="checkbox-label font-bold" style="text-align: center">EAD - GC
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="EadGc" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'EAD - GC',
                              'workAuthorization'
                            )
                          " name="EAD - GC" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose EAD - H4">
                          <label class="checkbox-label font-bold" style="text-align: center">EAD - H4
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="EadH4" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'EAD - H4',
                              'workAuthorization'
                            )
                          " name="EAD - H4" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose EAD - L2">
                          <label class="checkbox-label font-bold" style="text-align: center">EAD - L2
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="EadL2" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'EAD - L2',
                              'workAuthorization'
                            )
                          " name="EAD - L2" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose EAD - OPT">
                          <label class="checkbox-label font-bold" style="text-align: center">EAD - OPT
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="EadOpt" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'EAD - OPT',
                              'workAuthorization'
                            )
                          " name="EAD - OPT" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Green Card">
                          <label class="checkbox-label font-bold" style="text-align: center">Green Card
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="GreenCard" type="checkbox"
                              (click)="
                            chooseJobFilter($event, 'Green Card', 'workAuthorization')
                          " name="Green Card" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose H1B">
                          <label class="checkbox-label font-bold" style="text-align: center">H1B
                            <input [ngModelOptions]="{standalone: true}" [(ngModel)]="H1B" type="checkbox" (click)="
                            chooseJobFilter($event, 'H1B', 'workAuthorization')
                          " name="H1B" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Others">
                          <label class="checkbox-label font-bold" style="text-align: center">Others
                            <input type="checkbox" disabled (click)="
                            chooseJobFilter($event, 'Other', 'workAuthorization')
                          " name="Other" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose TN Visa">
                          <label class="checkbox-label font-bold" style="text-align: center">TN Visa
                            <input type="checkbox" [ngModelOptions]="{standalone: true}" [(ngModel)]="TnVisa" (click)="
                            chooseJobFilter($event, 'TN Visa', 'workAuthorization')
                          " name="TN Visa" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose US Citizen">
                          <label class="checkbox-label font-bold" style="text-align: center">US Citizen
                            <input type="checkbox" [ngModelOptions]="{standalone: true}" [(ngModel)]="UsCitizen"
                              (click)="
                            chooseJobFilter($event, 'US Citizen', 'workAuthorization')
                          " name="US Citizen" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>

                </div>
                <div class="row">
                  <div class="col-sm-3" style="padding-bottom: 7px">
                    <label for="location">Location</label>
                    <div class="row app-form">
                      <div class="col-sm-6" style="padding-bottom: 7px">
                        <select class="form-control input-sm" formControlName="country" type="text"
                          (change)="onChangeCountry($event.target.value)">
                          <option value="null" selected disabled>
                            Country
                          </option>
                          <option *ngFor="let country of countryList" [value]="country.countryCode">
                            {{ country.countryName }}
                          </option>
                        </select>
                      </div>
                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'US'">
                        <input type="text" placeholder="Zip Code" formControlName="zipcode"
                          class="form-control input-sm" (keyup)="onKeyZip($event)" />
                      </div>

                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'CA'">
                        <select class="form-control input-sm" formControlName="state" type="text">
                          <option value="null" selected="true" disabled>
                            State
                          </option>
                          <option *ngFor="let state of stateListCA" [value]="state.stateName">
                            {{ state.stateName }}
                          </option>
                        </select>
                      </div>

                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'IN'">
                        <select class="form-control input-sm" formControlName="state" type="text">
                          <option value="null" selected="true" disabled>
                            State
                          </option>
                          <option *ngFor="let state of stateListIN" [value]="state.stateName">
                            {{ state.stateName }}
                          </option>
                        </select>
                      </div>

                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'AU'">
                        <select class="form-control input-sm" formControlName="state" type="text">
                          <option value="null" selected="true" disabled>
                            State
                          </option>
                          <option *ngFor="let state of stateListAU" [value]="state.stateName">
                            {{ state.stateName }}
                          </option>
                        </select>
                      </div>

                      <div class="col-sm-6" *ngIf="
                      candidateFilterForm.value.country != 'AU' &&
                      candidateFilterForm.value.country != 'IN' &&
                      candidateFilterForm.value.country != 'CA' &&
                      candidateFilterForm.value.country != 'US'
                    ">
                        <input type="text" placeholder="State" formControlName="state" class="form-control input-sm" />
                      </div>
                    </div>
                    <div class="row form-group app-form">
                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'US'">
                        <input type="text" disabled placeholder="City" formControlName="city"
                          class="form-control input-sm" />
                      </div>

                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country != 'US'">
                        <input type="text" placeholder="City" formControlName="city" class="form-control input-sm" />
                      </div>

                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country != 'US'">
                        <input type="text" placeholder="zipcode" formControlName="zipcode"
                          class="form-control input-sm" />
                      </div>
                      <div class="col-sm-6" *ngIf="candidateFilterForm.value.country == 'US'">
                        <input type="text" disabled placeholder="State" formControlName="state"
                          class="form-control input-sm" />
                      </div>
                    </div>

                  </div>
                  <div class="col-sm-3">
                    <div class="row">
                      <div class="col-sm-12">
                        <label for="">Work Type</label>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Remote Work">
                          <label class="checkbox-label font-bold" style="text-align: center">Remote Work
                            <input formControlName="remoteWork" type="checkbox" (click)="
                            chooseJobFilter($event, 'remoteWork', 'workType')
                          " name="remoteWork" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>

                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Open To Relocate">
                          <label class="checkbox-label font-bold" style="text-align: center">Open To Relocate
                            <input formControlName="relocationRequired" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'relocationRequired',
                              'workType'
                            )
                          " name="relocationRequired" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                      <div class="col-sm-12">
                        <div class="custom-checkbox" title="Choose Work From Home">
                          <label class="checkbox-label font-bold" style="text-align: center">Work From Home
                            <input formControlName="workFromHome" type="checkbox" (click)="
                            chooseJobFilter(
                              $event,
                              'workFromHome',
                              'workType'
                            )
                          " name="workFromHome" />
                            <span class="checkmark"></span>
                          </label>
                        </div>
                      </div>
                    </div>
                  </div>
                  <div class="col-sm-6">
                    <div class="row">
                      <div class="col-sm-6">
                        <label for="">Total Experience (Years)</label>
                        <div class="custom-slider">
                          <ngx-slider (userChangeEnd)="onChangeYear($event)" [(value)]="value" defaultColor  [options]="options">
                          </ngx-slider>
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <label for="">Posted (Days)</label>
                        <div class="custom-slider">
                          <ngx-slider [(value)]="value1" [(highValue)]="highValue1" [options]="options1"></ngx-slider>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
              <div *ngIf="throwErrorFlag" style="color: #c3002f;; float: right;">No filters selected.</div>
            </div>
            <div class="panel-footer">
              <div class="row">
                <div class="text-right" style="
                padding-right: 49px;
                padding-top: 7px;
                padding-bottom: 7px;
              ">
                  <button class="btn default-btn btn-sm" (click)="resetFilter()">
                    Reset
                  </button>
                  <button class="btn primary-btn btn-sm" style="margin-left: 2%" type="button"
                    (click)="applyFilter()" (click)="response=[]">
                    Filter
                  </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <app-candidates-page [candidatesFoundStatus]="candidateFoundStatus" [passValues]="response" [candidateData]="filterdata" [removeSelected]="removeSelected"
      [removeTag]="removeTagvalues" (selectedCandidate)="getchildData($event)"></app-candidates-page>
    <div *ngIf="stopscrollFlag && response.length!=0" class="row caught_up">
      <div class="col-md-12 mg-bt-9">
        <img style="max-height: 33px;" src="assets/icon/caught_up.png" alt="">
      </div>
      <div class="col-md-12">
        <p class="biase">You've reached the bottom of the page</p>
      </div>
    </div>

    <router-outlet></router-outlet>
  </div>
  <div class="col-sm-3" [class.sticktherecss]="widgetstick" #widgetsticky style="padding-left: 5px;">
    <app-job-submit-skillfilter *ngIf="filterApplied" [candidateData]="filterdata"
      [dataPasstoSkillwidgets]="dataPasstoSkillwidgets" (emitRemoveTag)="removeUserfromTag($event)">
    </app-job-submit-skillfilter>
    <app-featured-jobs *ngIf="!filterApplied"></app-featured-jobs>
  </div>
</div>
