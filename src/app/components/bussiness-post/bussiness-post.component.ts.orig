        } else {
          res.data.response.forEach((element2) => {
            element2.checked = false;
          });
          this.checkedList.forEach(element1 => {
            res.data.response.forEach((element2) => {
              if (element1 === element2.userId) {
                element2.checked = true;
              } else {
                if (element1 === element2.userId) {
                  element2.checked = true;
