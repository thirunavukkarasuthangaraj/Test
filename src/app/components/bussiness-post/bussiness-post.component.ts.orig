              primaryEmail = res.email
              this.firstName = res.firstName
              this.lastName = res.lastName
              workExperience = res.workExperience
              this.primaryEmail = res.email
              if (res.secondaryEmail == null || res.secondaryEmail == undefined) {
                this.secondaryEmail = 'None provided'
